    var SI0501_isMapInit = false;
	scwin.onpageload = function() {
		var autoOptions = {
			Main : { grid : SI0501G0, frm : SI0501F0, srchSbm : sub_SI0501G0R0, savSbm : sub_SI0501G0S0, 
					 keySbm : sub_SI0501G0K0, srchGrp : pgm_header , keyColumn : "STTN_ID", focusColumn : "STTN_NM", name : "정류소 목록"},
			Sub1 : { grid : SI0501G1, srchSbm : sub_SI0501G1R0, name : "관련 노선" }
		 };
		 
		com.setMainBtn2(wfm_mainBtn, gcm.DISP_TYPE.DUAL_GRID_FORM, autoOptions);
		
		var codeOptions = [ {code : "AREA", compID : "AREA, SI0501G0:AREA" },
							{code : "VHC_DOOR_DIR_TYPE", compID : "VHC_DOOR_DIR_TYPE, SI0501G0:VHC_DOOR_DIR_TYPE" },
							{code : "STTN_FCLT_TYPE", compID : "STTN_FCLT_TYPE, SI0501G0:STTN_FCLT_TYPE" },
							{code : "BAY_TYPE", compID : "BAY_TYPE, SI0501G0:BAY_TYPE" },
							{code : "WAY_DIV", compID : "WAY_DIV, SI0501G0:WAY_DIV" },
							{code : "ROUT_TYPE", compID : "SI0501G1:ROUT_TYPE" }
													
						];		
	
		com.setCommonDtl(codeOptions, function() {
			com.searchGrid(SI0501G0, sub_SI0501G0R0);
		});	
		
		com.executeSubmission(sub_SI0501SHI0);
		
		//$().attr("style", "width:200px; height:27px;  float:left");
		//map_SI0501_div.hide();
		
		//탭 목록 클릭 이벤트
		$("#SI0501_TAB").find("li").click( function() {
			var li_id = this.id;
			$("#SI0501_TAB").find("li").removeClass("on");
			$("#SI0501_TAB").find("#"+li_id).addClass("on");
			
			if(li_id == "li_rout") {
				pgm_sub_subList.show("");
				map_SI0501_div.hide();
				//routMap.destroy("map_SI0501");
			}
			else if(li_id == "li_map") {
				pgm_sub_subList.hide();
				
				var val_REP_ROUT_ID = dlt_BMS_REP_ROUT_MST.getColData("REP_ROUT_ID");
				dma_sub_search.set("REP_ROUT_IDS", val_REP_ROUT_ID);
				dma_sub_search.set("TYPE", "DISP_NODE");
				com.executeSubmission(sub_SI0501G2R0);
				setTimeout(function() {
					if(SI0501_isMapInit==false){
						routMap.initMap("map_SI0501", {width:"100%"
							, height:"100%"
							, onClick: null
							, nodeChangeCb: null
							, draggable: false
						});
						SI0501_isMapInit = true;
					}
					routMap.showRoute("map_SI0501", BMS_ROUT_NODE_DISP_VW.getAllJSON(), dma_sub_search.get("STTN_ID"), "BUSSTOP");
				},100);
				map_SI0501_div.show("");
			}
		});
	};
	
	scwin.onpageunload = function() {
		
	};
	
	scwin.sub_SI0501G0R0_submitdone = function(e) {
		com.setFocusedCell(SI0501G0, "STTN_ID");
	};
	
	scwin.sub_SI0501G0S0_submitdone = function(e) {
		var sCode = com.getResultCode(e);
		if (sCode == gcm.MESSAGE_CODE.STATUS_SAVE) {
			com.saveDone(SI0501G0,sub_SI0501G0R0);
		}
	};
	
	scwin.sub_SI0501G0K0_submitdone = function(e) {
		com.addGridForm(SI0501G0, SI0501F0, dma_SEQ_BMS_STTN_MST_0, "STTN_ID", "AREA");
	};
	
	scwin.sbx_ChoiceBox_onviewchange = function(info) {
		if (info.newValue == "ALL") {
			autoChoiceBox.hide();
			search_input.setValue("");
			search_input.show("");
		} 
		else if (info.newValue == "STTN_ID") {
			search_input.hide();
			autoChoiceBox.setNodeSet("data:dlt_searchitem", "STTN_ID", "STTN_ID");
			autoChoiceBox.show("");
		} else if (info.newValue == "STTN_NM") {
			search_input.hide();
			autoChoiceBox.setNodeSet("data:dlt_searchitem", "STTN_NM", "STTN_NM");
			autoChoiceBox.show("");
		} else if (info.newValue == "STTN_NO") {
			search_input.hide();
			autoChoiceBox.setNodeSet("data:dlt_searchitem", "STTN_NO", "STTN_NO");	
			autoChoiceBox.show("");
		}
	};
	
	scwin.SI0501G0_onrowindexchange = function(row,oldRow) {
		var focusOption = {keyMap : dma_sub_search};
		com.changeDualGrid(SI0501G0, SI0501G1, null, sub_SI0501G1R0, focusOption, row, oldRow);
	};
	
	scwin.sub_SI0501G1R0_submitdone = function(e) {
		$("#SI0501_TAB").find("#li_map").removeClass("on");
		$("#SI0501_TAB").find("#li_rout").addClass("on");	
		pgm_sub_subList.show("");
		map_SI0501_div.hide();
	};
	
	scwin.sub_SI0501G2R0_submitdone = function(e) {
		
		//	routMap.showRoute("map_SI0501", BMS_ROUT_NODE_DISP_VW.getAllJSON(), dma_sub_search.get("STTN_ID"), "BUSSTOP");
	};
	
	scwin.YN = function(obj){
		if(obj == 'Y'){
			return "예";
		}else if(obj == 'N'){
			return "아니요";
		}
	}
	
	]]></script>
