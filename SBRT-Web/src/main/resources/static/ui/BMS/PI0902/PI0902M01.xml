	scwin.PI0902G1_oncellclick = function(row,col,colId) {
      if (col != "0"){
         if(PI0902G1.getCellChecked(row, 'chk')==0){
            PI0902G1.setCellChecked(row, 'chk', true);
         }else if(PI0902G1.getCellChecked(row, 'chk')==1){
            PI0902G1.setCellChecked(row, 'chk', false);
         }
      }		
	};
	
