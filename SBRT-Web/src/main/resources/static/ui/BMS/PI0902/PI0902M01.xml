	scwin.btn_resrv_onclick = function() {
		var jsondata = PI0902G1.getCheckedJSON("chk");
		var data = {data : jsondata, callbackFn : "scwin.PI0902P0_callbackFn"};
		if(jsondata.length > 0){
			var options = { id : "PI0902P01",
							popupName : "예약 날짜",
							modal : true,
							width : 220, height: 350};
			com.openPopup("/cm/common/popUpReserve.xml", options, data);
		}else{
			com.alert('예약할 목록을 선택해 주세요.');
		}
		

	};
	
	scwin.PI0902P0_callbackFn = function(obj) {
		var rows = PI0902G1.getCheckedIndex("chk");
		var count = rows.length;
		if (count > 0 ) {
			for(i = 0; i<count; i++) {
				dlt_VHC_MST.setCellData(rows[i], "RSV_DT", obj.data[i].RSV_DT);
			}
		}
		com.saveGrid(PI0902G1, sub_PI0902G1S0);	
	}
	
	scwin.btn_resrvcnl_onclick = function() {
		var rows = PI0902G1.getCheckedIndex("chk");
		var count = rows.length;
		var str = count + " 건의 데이터를 예약 취소 하시겠습니까?";
		
		if(count > 0) {
			com.confirm(str, function(rtn) {
				if (rtn) {
					com.executeSubmission(sub_PI0902G1U0);
				}	
			});
		}
		
	}
	
