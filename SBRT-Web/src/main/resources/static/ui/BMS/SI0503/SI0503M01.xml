		 };		
		 
		com.setMainBtn2(wfm_mainBtn, gcm.DISP_TYPE.SINGLE_GRID_FORM, autoOptions);
		
		var codeOptions = [ {code : "CRS_KIND", compID : "CRS_KIND, SI0503G0:CRS_KIND" },
							{code : "SIG_CTR_TYPE", compID : "SIG_CTR_TYPE, SI0503G0:SIG_CTR_TYPE" }
						];		
		
		com.setCommonDtl(codeOptions, function() {
			com.searchGrid(SI0503G0, sub_SI0503G0R0);
		});
		
		com.executeSubmission(sub_SI0503SHI0);

		routMap.initMap("map_SI0503", {width:"100%"
			, height:"100%"
			, onClick: scwin.onClickMap
			, nodeChangeCb: scwin.nodeChangeCb
			, draggable: false
		});
	};
	
	scwin.onpageunload = function() {
		
	};
	
	
	scwin.sub_SI0503G0R0_submitdone = function(e) {
		com.setFocusedCell(SI0503G0, "CRS_ID");
	};
	
	scwin.sub_SI0503G0S0_submitdone = function(e) {
		var sCode = com.getResultCode(e);
		if (sCode == gcm.MESSAGE_CODE.STATUS_SAVE) {
			com.saveDone(SI0503G0,sub_SI0503G0R0);
		}
		
	};

	scwin.sub_SI0503G0K0_submitdone = function(e) {
		com.addGridForm(SI0503G0, SI0503F0, dma_SEQ_BMS_CRS_MST_0, "CRS_ID", "CRS_NM");
	};

	scwin.sbx_ChoiceBox_onviewchange = function(info) {
		if (info.newValue == "ALL") {
			autoChoiceBox.hide();
			search_input.setValue("");
			search_input.show("");
		} 
		else if (info.newValue == "CRS_ID") {
			search_input.hide();
			autoChoiceBox.setNodeSet("data:dlt_searchitem", "CRS_ID", "CRS_ID");
			autoChoiceBox.show("");
		} else if (info.newValue == "CRS_NM") {
			search_input.hide();
			autoChoiceBox.setNodeSet("data:dlt_searchitem", "CRS_NM", "CRS_NM");
			autoChoiceBox.show("");
		}
	};
	
	scwin.SI0503G0_onrowindexchange = function(row,oldRow) {
		var focusOption = {keyMap : dma_sub_search};
		com.changeDualGrid(SI0503G0, SI0503G1, null, sub_SI0503G1R0, focusOption, row, oldRow);

	};
	
	scwin.sub_SI0503G1R0_submitdone = function(e) {
		var val_ROUT_ID = dlt_BMS_ROUT_MST.getColData("ROUT_ID");
		dma_sub_search.set("ROUT_IDS", val_ROUT_ID);
		dma_sub_search.set("TYPE", "DISP_NODE");
		com.executeSubmission(sub_SI0503G2R0);
	};

	scwin.sub_SI0503G2R0_submitdone = function(e) {
		routMap.showRoute("map_SI0503", dlt_BMS_ROUT_NODE_CMPSTN.getAllJSON(), dma_sub_search.get("CRS_ID"), "CROSS");
	};
	

	]]></script>
