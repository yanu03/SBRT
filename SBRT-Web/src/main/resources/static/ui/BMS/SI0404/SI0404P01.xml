		<xf:submission id="sub_SI0404P0R0" ref="data:json,dma_search" target="data:json,dlt_BMS_NODE_MST" action="/si/SI0404P0R0"
			method="post" mediatype="application/json" encoding="UTF-8" instance="" replace="" errorHandler="" customHandler="" mode="asynchronous"
			processMsg="" ev:submit="" ev:submitdone="scwin.sub_SI0404P0R0_submitdone" ev:submiterror="" abortTrigger="">
		</xf:submission>
	</xf:model>
	<script type="text/javascript" lazy="false"><![CDATA[

	var isConfirmBtnClick = false;
	scwin.onpageload = function() {
		isConfirmBtnClick = false;
		scwin.param = com.getParameter();
		dma_search.set("ROUT_ID" , scwin.param.data.ROUT_ID);
		dma_search.set("INT_ROUT_ID" , scwin.param.data.INT_ROUT_ID);
		dma_search.set("NODE_ECPT_ID" , scwin.param.data.NODE_IDS);
		scwin.closeCallbackFncName = scwin.param.callbackFn;
		com.setEnterKeyEvent(popup_header, scwin.btn_search_onclick);
		var btnOptions = 
		[
			{userStr:"조회", auth: gcm.BTN.SEARCH, cbFnc: scwin.btn_search_onclick}
		];
		com.setMainBtn3(wfm_mainBtn, btnOptions);

		var codeOptions = [{ code : "NODE_TYPE", compID : "sbx_NodeType, SI0404P0:NODE_TYPE"}];

		com.setCommonDtl(codeOptions, function() {
			scwin.btn_search_onclick();
		});
						
	};
	
	scwin.onpageunload = function() {
		
	};
	
	
	scwin.btn_search_onclick = function() {
		com.executeSubmission(sub_SI0404P0R0);
	};
	
	scwin.btn_confirm_onclick = function() {
		console.log("btn_confirm_onclick isConfirmBtnClick="+isConfirmBtnClick);
		if(isConfirmBtnClick==false){
			isConfirmBtnClick = true;
			var data = SI0404P0.getCheckedJSON("chk");
			com.closePopup(scwin.closeCallbackFncName, data);
		}	
	};
	
	scwin.btn_cancel_onclick = function() {
		com.closePopup();		
	};
	scwin.SI0404P0_oncellclick = function(row,col) {
		if(col==0)return;
		if(SI0404P0.getCellChecked(row,"chk")){
			SI0404P0.setCellChecked(row,"chk",false)
		}
		else {
			SI0404P0.setCellChecked(row,"chk",true)
		}
	};
	
	scwin.sub_SI0404P0R0_submitdone = function(e) {
		var sCode = com.getResultCode(e);
		if (sCode == gcm.MESSAGE_CODE.STATUS_SUCCESS) {
		
			var ids=dma_search.get("NODE_ECPT_ID");
			com.excludeItemsByGrid(SI0404P0, ids, "NODE_ID");
			
			var rowData = dlt_BMS_NODE_MST.getAllJSON();
			for(var i = 0; i < rowData.length; i++) { //노선의 노드 순번을 그리드 순서대로 재 할당함.
				SI0404P0.setCellChecked(i,"chk", true);
			}
		}
	};
	
	scwin.sbx_NodeType_onchange = function(row,oldRow) {
		scwin.btn_search_onclick();
	};
	]]></script>
	</head>
	<body ev:onpageload="scwin.onpageload" ev:onpageunload="scwin.onpageunload">
