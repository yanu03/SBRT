		<script type="text/javascript" lazy="false"><![CDATA[
	var isSizeCheck = false;
	scwin.onpageload = function() {
		
		var autoOptions = {
			Main : { grid : PI0701G0, srchSbm : sub_PI0701G0R0, savSbm : sub_PI0701G0S0, 
					srchGrp : pgm_header, keyColumn : "ROUT_ID", name : "노선 목록"}
		 };		
	
		var codeOptions = [ {code : "WAY_DIV", compID : "PI0701G0:WAY_DIV" }
							,{code : "EFFECT_TYPE", compID : "PI0701G1:EFFECT_TYPE" }
						];		
		
		com.executeSubmission(sub_PI0701SHI0);		
		com.executeSubmission(sub_PI0701SHI1);	
				
		var mainOptions = {
			SETTING : scwin.btn_setting_onclick
		}
		
		com.setMainBtn2(wfm_mainBtn, gcm.DISP_TYPE.SINGLE_GRID_FORM, autoOptions, mainOptions);	
		
		com.setCommonDtl(codeOptions, function() {
			com.searchGrid(PI0701G0, sub_PI0701G0R0);
			for (i=0; i<10; i++) {
				dlt_actionSetting.insertRow(i);
				dlt_actionSetting.setCellData(i, "FRAME_NO", "FRAME"+i);
				dlt_actionSetting.setCellData(i, "EFFECT_TYPE", "ET001");
				dlt_actionSetting.setCellData(i, "EFFECT_SPEED", "05");
				dlt_actionSetting.setCellData(i, "SHOW_TIME", "0000");
				dlt_actionSetting.modifyRowStatus(i, "R");
//				var tmpJSON = [{"FRAME_NO":"FRAME"+i, "EFFECT_TYPE":"ET001", "EFFECT_SPEED":"05", "SHOW_TIME":"0000"}];
			}
		});
	
	};

	scwin.onpageunload = function() {

	};
	
	scwin.sub_PI0701G0R0_submitdone = function(e) {
		com.setFocusedCell(PI0701G0, "ROUT_ID");
	};
	
	scwin.upload0_onInputValueChange = function(returnValue){
		
		// 확장자 추출
		var lastDot = returnValue.lastIndexOf('.'); 
		var fileExtension = returnValue.substring(lastDot+1, returnValue.length);
		
		//파일명 추출
		var fileNameIndex = returnValue.lastIndexOf('\\');
		var fileName = returnValue.substring(fileNameIndex+1, returnValue.length);
		
		if (fileExtension == "BMP" || fileExtension == "bmp") {
			
			dlt_ROUT_MST.setCellData(PI0701G0.getFocusedRowIndex(), "FILE_NM", fileName);
			btn_upload.submit();
		} else {
			com.alert("BMP 확장자만 업로드 할 수 있습니다.");
			btn_upload.reset();	
		}
				
		isSizeCheck = true;
	}
	
	scwin.upload0_ondone = function(retXml) {
		var real_fileName = dlt_ROUT_MST.getCellData(PI0701G0.getFocusedRowIndex(), "FILE_NM");
		var previewPath = "/fileUpload/up/"+real_fileName;
		$p.$("#image1").attr("src", previewPath);
		
		//이미지 로드 후 이벤트 실행
		$p.$("#image1").on('load', function(e) {
			var x = dlt_searchitem2.getCellData(DL_CD.selectedIndex, "NUM_VAL4") * dlt_searchitem2.getCellData(DL_CD.selectedIndex, "NUM_VAL5");
			var y = dlt_searchitem2.getCellData(DL_CD.selectedIndex, "NUM_VAL4") * dlt_searchitem2.getCellData(DL_CD.selectedIndex, "NUM_VAL6");		
			var image_width = $p.$("#image1").width();
			var image_height = $p.$("#image1").height();
			
			//이미지 사이즈 체크
			if (isSizeCheck == true &&((image_width % x != 0) || (image_height % y != 0))) {
				
				com.alert("이미지의 사이즈가 맞지 않습니다.");
				btn_upload.reset();
				dlt_ROUT_MST.undoAll();
				com.executeSubmission(sub_PI0701G0R0);
				isSizeCheck = false;
				
			}
			return false;				
		});
				
	};
	
	
	//파일변경, 동작설정 후 onrowindexchange시 alert창 띄워서 막아야 함
	scwin.PI0701G0_onrowindexchange = function(row,oldRow) {
		var main_idx = PI0701G0.getModifiedIndex().length;
		var sub_idx = PI0701G1.getModifiedIndex().length;		
		
//		if (main_idx > 0 || sub_idx > 0) {
//			com.confirm("저장되지 않은 데이터가 있습니다. 저장하시겠습니까?", function(rtn){
//				if (rtn) {
//					$p.executeSubmission(sub_PI0701G0S0);
//				} 
//			});
//		} 
			
		var jsonData = dlt_ROUT_MST.getAllJSON()[row];
		dma_ROUT_MST.setJSON(jsonData);
		showImage();
		com.executeSubmission(sub_PI0701G1R0);			
	};
	
	scwin.sbx_ChoiceBox_onviewchange = function(info) {
		if (info.newValue == "ALL") {
			autoChoiceBox.hide();
			search_input.setValue("");
			search_input.show("");
		} 
		else if (info.newValue == "ROUT_ID") {
			search_input.hide();
			autoChoiceBox.setNodeSet("data:dlt_searchitem", "ROUT_ID", "ROUT_ID");
			autoChoiceBox.show("");
		}
		else if (info.newValue == "ROUT_NM") {
			search_input.hide();
			autoChoiceBox.setNodeSet("data:dlt_searchitem", "ROUT_NM", "ROUT_NM");
			autoChoiceBox.show("");
		} 		
	};
	
	scwin.btn_setting_onclick = function() {
		var main_idx = PI0701G0.getModifiedIndex().length;
		var sub_idx = PI0701G1.getModifiedIndex().length;
		
		if (main_idx > 0 || sub_idx > 0 ) {
			com.confirm("설정 하시겠습니까?", function(rtn) {
				if (rtn) {
					$p.executeSubmission(sub_PI0701G0S0);
				}	
			});
		} else {
			com.alert("파일 등록 후 설정 하실 수 있습니다.");	
			return false;			
		}
	}
	
	//파일변경, 동작설정 후 onchange시 alert창 띄워서 막아야 함
	scwin.DL_CD_onchange = function(info) {
		var main_idx = PI0701G0.getModifiedIndex().length;
		var sub_idx = PI0701G1.getModifiedIndex().length;		
		
		if (main_idx > 0 || sub_idx > 0) {
			com.confirm("저장되지 않은 데이터가 있습니다. 저장하시겠습니까?", function(rtn){
				if (rtn) {
					$p.executeSubmission(sub_PI0701G0S0);
				}
				else {
					showImage();
					$p.executeSubmission(sub_PI0701G1R0);
				}
			});
		} 
		else {
			showImage();
			$p.executeSubmission(sub_PI0701G1R0);
		}
	};
	
	scwin.sub_PI0701FCK0_submitdone = function(e) {
		var fileCheck = dma_filechk.get("CHK");
		var fileurl = dma_fileinfo.get("FILE_PATH");
		
		if(fileCheck == "1") {
			var fileNames = fileurl.split("/");
			$p.$("#mf_tac_layout_contents_MN00040801_body_btn_upload_inputFile").attr("value", fileNames[fileNames.length-1]);
		} else if(fileCheck == "0"){
			$p.$("#mf_tac_layout_contents_MN00040801_body_btn_upload_inputFile").attr("value", null);			
			$p.$("#mf_tac_layout_contents_MN00040801_body_btn_upload_inputFile").attr("placeholder", "등록된 파일이 없습니다.");		
		}		
		
	};
	
	//파일 읽기
	function showImage() {
		
		var dl_cd = DL_CD.getValue();
		var file_header = dlt_searchitem2.getCellData(DL_CD.selectedIndex, "TXT_VAL2");
		var dvc_nm = dma_ROUT_MST.get("DVC_NM");
		var file_nm = file_header + dvc_nm + ".BMP";
		var sch_file_nm = file_header + dvc_nm + ".SCH";
		dma_ROUT_MST.set("FILE_NM", file_nm);
		dma_ROUT_MST.set("SCH_FILE_NM", sch_file_nm);
		dma_ROUT_MST.set("SELECTED_DL", dl_cd);
		
		var path = "/fileUpload/temp/destination/"+dl_cd+"/"+file_nm;
		//var previewPath = "/fileUpload/up/"+file_nm;
		dma_ROUT_MST.set("FILE_PATH", path);
		
		//sub_PI0701FCK0 용도
		dma_fileinfo.set("FILE_PATH", path);
		com.executeSubmission(sub_PI0701FCK0);
		
		$p.$("#image1").attr("src", path);
		$p.$("#image1").on('load', function(e) { 
			setGridSize();
		}); 		
	}
	
	//이미지 사이즈, 그리드 후처리
	function setGridSize() {
		var frame_x = dlt_searchitem2.getCellData(DL_CD.selectedIndex, "NUM_VAL4") * dlt_searchitem2.getCellData(DL_CD.selectedIndex, "NUM_VAL5");
		var frame_y = dlt_searchitem2.getCellData(DL_CD.selectedIndex, "NUM_VAL4") * dlt_searchitem2.getCellData(DL_CD.selectedIndex, "NUM_VAL6");
		var frame_xy = frame_x * frame_y;
		//$p.$("#image1").width(x);
		var imageHeight = $p.$("#image1").height();
		var activeRowCount = imageHeight / frame_y;

		//기본 이미지인지 체크
		var checkErrImage = $p.$("#image1").attr("src");
		if (checkErrImage == "/cm/images/notice/noimg.jpg") {
			for (i=0; i<10; i++) {
				PI0701G1.setRowColor(i, "#68717b");
				PI0701G1.setRowReadOnly(i, true);				
			}
				return false;
		}

		for (i=0; i<activeRowCount; i++) {
			PI0701G1.setRowReadOnly(i, false);
			PI0701G1.setRowColor(i, "red");
		}
		
		for (i=activeRowCount; i<10; i++) {
			PI0701G1.setRowColor(i, "#68717b");
			PI0701G1.setRowReadOnly(i, true);			
		} 
		
	}

	scwin.sub_PI0701G0S0_submitdone = function(e) {
		//com.saveDone(PI0701G0, sub_PI0701G0R0);
		showImage();
		com.executeSubmission(sub_PI0701G0R0);
	};
	
	scwin.sub_PI0701G1R0_submitdone = function(e) {
		setGridSize();
		isAlert = false;
	};
	]]></script>
