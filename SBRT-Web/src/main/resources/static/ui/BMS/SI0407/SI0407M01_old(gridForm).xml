			else if(dlt_BMS_MOCK_NODE.getCellData(i, "TURN_P") == "1") {
				dlt_BMS_MOCK_NODE.setCellData(i, "TURN_P", "예");
			}
		}		
	};
	
	scwin.SI0407G0_onrowindexchange = function(row,oldRow) {
		linkNode = row;
		com.changeSingleGrid(SI0407G0, row, oldRow);		
	};
