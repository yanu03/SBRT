		if (info.newValue == "ALL") {
			autoChoiceBox2.hide();
			search_input2.setValue("");
			search_input2.show("");
		} 
		else if (info.newValue == "MOCK_LINK_ID") {
			search_input2.hide();
			autoChoiceBox2.setNodeSet("data:dlt_searchitem2", "MOCK_LINK_ID", "MOCK_LINK_ID");
