		if (info.newValue == "ALL") {
			autoChoiceBox.hide();
			search_input.setValue("");
			search_input.show("");
		} 
		else if (info.newValue == "MOCK_NODE_ID") {
			search_input.hide();
			autoChoiceBox.setNodeSet("data:dlt_searchitem", "MOCK_NODE_ID", "MOCK_NODE_ID");
			autoChoiceBox.show("");
			
		} else if (info.newValue == "NODE_NAME") {
			search_input.hide();
			autoChoiceBox.setNodeSet("data:dlt_searchitem", "NODE_NAME", "NODE_NAME");
			autoChoiceBox.show("");
		}		
	};
	
