		};
		
		var mainOptions = {
			RESERV : scwin.btn_resrv_onclick,
			RESERV_CNL : scwin.btn_resrvcnl_onclick
		};
		
		//버튼 생성
		com.setMainBtn2(wfm_mainBtn, gcm.DISP_TYPE.SINGLE_GRID, autoOptions, mainOptions);
		
		//코드옵션
		//(code : DataCollection의 컬럼 ID명, compID : "FormId , GridId : GridColumn")
		//Grid일 경우 inputType이 select여야 함. form일경우 selectBox여야 함.
		var codeOptions = [ {code : "ORG_DIV", compID : "VD0203G0:ORG_DIV"}
							,{code : "INST_LOC", compID : "VD0203G0:INST_LOC"}
							,{code : "DVC_KIND", compID : "VD0203G0:DVC_KIND"}
							,{code : "PROCE_RST", compID : "VD0203G0:PROCE_RST"}
						];
		
		//코드옵션 실행
		com.setCommonDtl(codeOptions, function() {
			com.searchGrid(VD0203G0, sub_VD0203G0R0);
		});
		
		
		VD0203G0.mergeCol(0,false);
		VD0203G0.mergeCol(2,false);
		VD0203G0.mergeCells("byrestriccol");
		
//		$p.executeSubmission(sub_SI0101SHI0);
		$p.executeSubmission(sub_VO0203SHI0);
	};

