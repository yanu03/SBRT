		//Grid일 경우 inputType이 select여야 함. form일경우 selectBox여야 함.
		var codeOptions = [ //{code : "ORG_DIV", compID : "VD0203G0:ORG_DIV"}
							{code : "INST_LOC", compID : "VD0203G0:INST_LOC"}
							,{code : "DVC_KIND", compID : "sbx_DVC_KIND, VD0203G0:DVC_KIND"}
							,{code : "PROCE_RST", compID : "VD0203G1:PROCE_RST"}
							//,{code : "ORG_DIV", compID : "VD0203G1:ORG_DIV"}
							,{code : "DVC_KIND", compID : "VD0203G1:DVC_KIND"}
							,{code : "INST_LOC", compID : "VD0203G1:INST_LOC"}
						];
		//코드옵션 실행
		com.setCommonDtl(codeOptions, function() {
			com.searchGrid(VD0203G0, sub_VD0203G0R0);
		});
		
		com.executeSubmission(sub_VD0203SHI0);
		//acb_autoChoiceBox.setNodeSet("data:dlt_searchitem", "VHC_NO", "VHC_NO");
