	scwin.file_onInputValueChange = function(returnValue) {
		// 확장자 추출
		var lastDot = returnValue.lastIndexOf('.'); 
		var fileExtension = returnValue.substring(lastDot+1, returnValue.length);
		
		//파일명 추출
		var fileNameIndex = returnValue.lastIndexOf('\\');
		var fileName = returnValue.substring(fileNameIndex+1, returnValue.length);
		
		dlt_DVC_INFO.setCellData(VD0203G0.getFocusedRowIndex(), "FILE_NM", fileName);
		dlt_DVC_INFO.setCellData(VD0203G0.getFocusedRowIndex(), "FILE_EXTENSION", fileExtension);
		dma_fileinfo.set("FILE_EXTENSION", fileExtension);

		div_file.submit();		
	};
	
	scwin.VD0203G0_onrowindexchange = function(row,oldRow) {
		var path = dlt_DVC_INFO.getCellData(VD0203G0.getFocusedRowIndex(), "FILE_PATH");		
		dma_fileinfo.set("FILE_PATH", path);		
		
		com.executeSubmission(sub_VD0203FCK0);
		
		var focusOption = {keyMap : dma_subsearch};
		com.changeDualGrid(VD0203G0, VD0203G1, null, sub_VD0203G1R0, focusOption, row, oldRow);
		
	};

