	//예약버튼 클릭 이벤트
	scwin.btn_resrv_onclick = function() {
		var jsondata = VD0203G0.getCheckedJSON("chk");
		
		if(jsondata.length > 0){
			var data = {data : jsondata, callbackFn : "scwin.VD0203P0_callbackFn"};
			if(jsondata.length > 0){
				var options = { id : "VD0203P01",
								popupName : "예약 날짜",
								modal : true,
								width : 220, 
								height: 350					
								};
				com.openPopup("/ui/BMS/VD0203/VD0203P01.xml", options, data);
			}
		
		}else{
			com.alert("업데이트할 장치를 선택해주세요.");
		}
		
	};
	
	scwin.VD0203P0_callbackFn = function(obj) {
		
		var rows = VD0203G0.getCheckedIndex("chk");
		var count = rows.length;
		if (count > 0 ) {
			for(i = 0; i<count; i++) {
				
				dlt_DVC_INFO.setCellData(rows[i], "RSV_DT", obj.data[i].RSV_DT);
			}
		}
		com.saveGrid(VD0203G0, sub_VD0203G0S0);	
	}
	
	scwin.btn_resrvcnl_onclick = function() {
		var rows = VD0203G0.getCheckedIndex("chk");
		
		if(rows.length > 0){
			var count = rows.length;
			var str = count + " 건의 데이터를 예약 취소 하시겠습니까?";
			
			if(count > 0) {
				com.confirm(str, function(rtn) {
					if (rtn) {
						com.executeSubmission(sub_VD0203G0D0);
					}	
				});
			}
		
		}else{
			com.alert("예약취소할 장치를 선택해주세요.");
		}
		
		
	}

	scwin.sbx_ChoiceBox_onviewchange = function(info) {		
		if (info.newValue == "ALL") {
			acb_autoChoiceBox.hide();
			search_input.setValue("");
			search_input.show("");
		} else if (info.newValue == "VHC_NO") {
			search_input.hide();
			acb_autoChoiceBox.setNodeSet("data:dlt_searchitem", "VHC_NO", "VHC_NO");
			acb_autoChoiceBox.show("");
		} else if (info.newValue == "COMPLETE_YN") {
			search_input.hide();
			acb_autoChoiceBox.setNodeSet("data:dlt_searchitem", "COMPLETE_YN", "COMPLETE_YN");
			acb_autoChoiceBox.show("");
		} 
	};
	
