	scwin.sub_VD0203FCK0_submitdone = function(e) {
		
		var fileCheck = dma_filechk.get("CHK");
		var fileurl = dma_fileinfo.get("FILE_PATH");
		
		var param = com.getParameter();
		
		var fileDispId = "#mf_tac_layout_contents_"+param.menuCode+"_body_div_file_inputFile";
		
		if(fileCheck == "1") {
			var fileNames = fileurl.split("/");
			$p.$(fileDispId).attr("value", fileNames[fileNames.length-1]);
		} else if(fileCheck == "0"){
			$p.$(fileDispId).attr("value", null);			
			$p.$(fileDispId).attr("placeholder", "등록된 파일이 없습니다.");		
		}
	};
	
	
