	<script type="text/javascript" lazy="false"><![CDATA[
	scwin.onpageload = function() {
		var autoOptions = {
			Main : { grid : FM0100G0, frm : FM0100F0, srchSbm : sub_FM0100G0R0, srchGrp : pgm_header, keyColumn : "NODE_ID",
					 focusColumn : "NODE_NM", name :  "노드 목록"},
			Sub1 : { grid : FM0100G1, srchSbm : sub_FM0100G1R0, savSbm : sub_FM0100G1S0,  keySbm : sub_FM0100G1K0, name :  "시설물 목록"}
		 };
		 
		com.setMainBtn2(wfm_mainBtn, gcm.DISP_TYPE.DUAL_GRID_FORM2, autoOptions);
		
//		var subOptions = {
//			ADD: {}, DEL:{}, CNL:{}
//		};
//		com.setSubBtn2(wfm_subBtn, autoOptions, subOptions);
		
		var codeOptions = [ {code : "NODE_TYPE", compID : "FM0100G0:NODE_TYPE" }
							,{code : "INST_LOC", compID : "INST_LOC, FM0100G1:INST_LOC" }
							,{code : "FCLT_KIND", compID : "FCLT_KIND, FM0100G1:FCLT_KIND"}	
							
		 ];
		 
		com.setCommonDtl(codeOptions, function() {
			com.searchGrid(FM0100G0, sub_FM0100G0R0);
		});
						 
		$p.executeSubmission(sub_FM0100SHI0);	
		$p.executeSubmission(sub_FM0100SHI1);
		$p.executeSubmission(sub_FM0100SHI2);	
	};

	scwin.onpageunload = function() {

	};

	//조회 콜백 메소드
	scwin.sub_FM0100G0R0_submitdone = function(e) {
		com.setFocusedCell(FM0100G0, "NODE_ID");
	};
	
	//그리드 저장 후 조회
	scwin.sub_FM0100G1S0_submitdone = function(e) {
		com.saveDone(FM0100G1,sub_FM0100G1R0);
	};
	
	//키값 추가 submission done
	scwin.sub_FM0100G1K0_submitdone = function(e) {
		com.addGridForm(FM0100G1, FM0100F0, dma_SEQ_BMS_FCLT_INFO_0, "FCLT_ID", "FCLT_ID");
		
		//VHC_ID 가져와서 SET
//		var focusIndex = FM0100G1.getFocusedRowIndex();
//		dlt_BMS_DVC_INFO.setCellData(focusIndex, "VHC_ID", dlt_BMS_VHC_MST.getCellData(VD0100G0.getFocusedRowIndex(), "VHC_ID"));

	};	
	
	scwin.sub_FM0100G0S0_submitdone = function(e) {
		var sCode = com.getResultCode(e);
		if (sCode == gcm.MESSAGE_CODE.STATUS_SAVE) {
			com.saveDone(FM0100G0,sub_FM0100G0R0);
		}
	};
		
		

	scwin.sbx_ChoiceBox_onviewchange = function(info) {
		
		if (info.newValue == "ALL") {
			autoChoiceBox.hide();
			search_input.setValue("");
			search_input.show("");
		} else if (info.newValue == "NODE_ID") {
			search_input.hide();
			autoChoiceBox.setNodeSet("data:dlt_searchitem", "NODE_ID", "NODE_ID");
			autoChoiceBox.show("");
		} else if (info.newValue == "NODE_NM") {
			search_input.hide();
			autoChoiceBox.setNodeSet("data:dlt_searchitem", "NODE_NM", "NODE_NM");
			autoChoiceBox.show("");
		}	
	};
	

	scwin.FM0100G0_onrowindexchange = function(row,oldRow) {
		var focusOption = {keyMap : dma_subsearch};
		com.changeDualGrid(FM0100G0, FM0100G1, sub_FM0100G1S0, sub_FM0100G1R0, focusOption, row, oldRow);
		
		var nodeType = dlt_BMS_NODE_MST.getCellData(row, "NODE_TYPE");
		
		if(nodeType == "NT001"){
			INST_LOC.setDisabled(true);
			//FCLT_KIND.setNodeSet("data:dlt_BMS_FCLT_INFO", "", "");
		}else if(nodeType == "NT002"){
			INST_LOC.setDisabled(false);
			//FCLT_KIND.setNodeSet(nodeset, label, value);
			
			//selectbox1.setNodeSet('data:dataList1','lbl','code');
		}
	};
	
	scwin.sub_FM0100G1R0_submitdone = function(e) {
		com.setFocusedCell(FM0100G1, "FCLT_ID");		
	};
	
	scwin.sub_VD0100G1R0_submitdone = function(e) {
		com.setFocusedCell(FM0100G1, "FCLT_ID");		
	};
	
	//노드ID 넣어주는 스크립트
	scwin.FM0100G1_onrowindexchange = function(row,oldRow) {
		dlt_BMS_FCLT_INFO.setCellData(row, "NODE_ID", dma_subsearch.get("NODE_ID"));
	};
	
	scwin.BUY_DT_onviewchange = function(info) {
		var instdt = INST_DT.getValue();
		var buydt = BUY_DT.getValue();
		
		
		if (instdt < buydt && instdt != ""){
			com.messagBox("alert", "지정할 수 없는 날짜입니다.", scwin.checkCloseBUY(info));
		}
	};
	scwin.checkCloseBUY = function(info) {
		BUY_DT.setValue(info.oldValue);
	}
	scwin.checkCloseINST = function(info) {
		INST_DT.setValue(info.oldValue);
	}
	
	scwin.INST_DT_onviewchange = function(info) {
		var instdt = INST_DT.getValue();
		var buydt = BUY_DT.getValue();
		
		if (buydt > instdt){
			com.messagBox("alert", "지정할 수 없는 날짜입니다.", scwin.checkCloseINST(info));
		}
	};
	
	//위치좌표 팝업
	scwin.TEMP_COORDS_onclick = function(e) {
		
		var data = { FCLT_ID : dlt_BMS_FCLT_INFO.getCellData(FM0100G1.getFocusedRowIndex(), "FCLT_ID"),
						FCLT_KIND : dlt_BMS_FCLT_INFO.getCellData(FM0100G1.getFocusedRowIndex(), "FCLT_KIND"),
						STTN_FCLT_TYPE : dlt_BMS_NODE_MST.getCellData(FM0100G0.getFocusedRowIndex(), "STTN_FCLT_TYPE"),
						FCLT_COORDS : dlt_BMS_FCLT_INFO.getCellData(FM0100G1.getFocusedRowIndex(), "FCLT_COORDS"),
				   	 	callbackFn : "scwin.searchCOORDSCallback" };
				   	 
		var opt = { id : "FM0100P01", 
					popupName : "시설물 위치 좌표 설정",
					modal : true,  
					width : 1400, height: 450 };
					
		if(data.FCLT_KIND != null && data.FCLT_KIND != ''){
			com.openPopup("/ui/BMS/FM0100/FM0100P01.xml", opt, data);
		}else{
			com.messagBox("alert", "시설물 유형을 선택해주세요.");
		}
	};
	
	//위치 좌표 버튼 콜백함수
	scwin.searchCOORDSCallback = function(returnValue) {
		dlt_BMS_FCLT_INFO.setCellData(FM0100G1.getFocusedRowIndex(), "FCLT_COORDS", returnValue);
		
	};
	
	scwin.btn_searchMro_onclick = function(e) {
	
		var data = { callbackFn : "scwin.searchMroCallback" };
				   	 
		var opt = { id : "FM0100P01", 
					popupName : "유지보수 기관 검색",
					width : 650,
					height : 500,
					modal : true};
		com.openPopup("/cm/common/popUpEmerClsfctn.xml", opt, data);		
	};
	
	scwin.searchMroCallback = function(returnValue) {
		CLSFCTN_ID.setValue(returnValue.CLSFCTN_NM);
		dlt_BMS_FCLT_INFO.setCellData(FM0100G1.getFocusedRowIndex(), "CLSFCTN_ID", returnValue.CLSFCTN_ID);
	};
	
	scwin.CLSFCTN_ID_onfocus = function(e) {
		scwin.btn_searchMro_onclick();		
	};
	
	]]></script>
