	scwin.onpageunload = function() {

	};
	
	/**
	 * 저장 버튼 클릭
	 */
	scwin.saveDvc = function() {
		dlt_BMS_DVC_INFO.getColData("VHC_ID");
		dlt_BMS_DVC_INFO.getColData("DVC_KIND");
		dlt_BMS_DVC_INFO.getColData("INST_LOC");
		
		var rowCount = dlt_BMS_DVC_INFO.getRowCount();
		
		var dupli = true;
		
		for(var i = 0; i < rowCount; i++){
			var dvcKind = dlt_BMS_DVC_INFO.getCellData(i, "DVC_KIND");
			var instLoc = dlt_BMS_DVC_INFO.getCellData(i, "INST_LOC");
			
			var comp = dvcKind + instLoc
			
			var dvcKind_sub = dlt_BMS_DVC_INFO.getCellData(i+1, "DVC_KIND");
			var instLoc_sub = dlt_BMS_DVC_INFO.getCellData(i+1, "INST_LOC");
			
			var comp_sub = dvcKind_sub + instLoc_sub
			
			if(comp == comp_sub){
				dupli = false;
			}
			
		}
		
		if(dupli){
			com.saveGrid(VD0100G1, sub_VD0100G1S0);
		}else{
			com.alert("중복된 장치종류와 설치위치가 존재합니다. 설치위치를 확인해 주십시오.");
		}
		
	}
	
	/**
	 * 장치 추가 버튼 클릭시 장치ID 키 값 생성 서브미션 실행
	 */
	scwin.execute_key = function() {
		com.executeSubmission(sub_VD0100G1K0)
	}
	
	/**
	 * 장치 추가 버튼 클릭
	 */
	scwin.btn_addDvc_onclick = function() {
		var menu_cnt = dlt_BMS_DVC_INFO.getTotalRow();

		if (menu_cnt == 0) {
			com.alert("조회 후 이용하세요.");
			return;
		}

		if (VD0100G1.getFocusedRealRowIndex() === -1) {
			VD0100G1.setFocusedCell(0, "DVC_ID");
		}
		
		var currMenuNm = dlt_BMS_DVC_INFO.getCellData(VD0100G1.getFocusedRealRowIndex(), "DVC_KIND_NM");
		var currMenuLevel = WebSquare.util.getNumber(dlt_BMS_DVC_INFO.getCellData(VD0100G1.getFocusedRealRowIndex(), "LEVEL"));
		
		if (menu_cnt != 0 && VD0100G1.getFocusedRowIndex() == -1) {
			com.alert("메뉴코드를 클릭하셔야 합니다.");
		} else {
			if (currMenuLevel < 2) {
				scwin.selectLevel(currMenuNm);
			} else {
				scwin.equalLevel();
			}
		}
	};
	
	/**
	 * 장치레벨이 1일 경우에 실행되는 함수
	 * Yes, No에 따라서 하위에 만들 것인지, 동일한 레벨로 만들 것인지 결정한다.
	 */
	scwin.selectLevel = function(returnValue) {
		com.confirm(returnValue + " 장치 하위에 새로운 장치를 추가하시겠습니까?\n 아니오를 누르면 동일한 레벨에 장치가 생성됩니다.", "scwin.selectLevelCallbak");
	};
	
	scwin.selectLevelCallbak = function(value) {
		if (value == true) {
			scwin.lowerLevel();
		} else {
			scwin.equalLevel();
		}
	};
	
	/**
	 * 하위 레벨의 노드에 메뉴를 추가한다.(포커스가 잡혔는데 depth가 1일 경우)
	 */
	scwin.lowerLevel = function() {
		var focusedRowIndex = VD0100G1.getFocusedRealRowIndex();
		var parentMenuCd = dlt_BMS_DVC_INFO.getCellData(focusedRowIndex, "PARENT_ID");
		var menuLevel = WebSquare.util.getNumber(dlt_BMS_DVC_INFO.getCellData(focusedRowIndex, "LEVEL")) + 1;
		var sortOrder = 1
		var insertRowIndex = scwin.getInsertRowIndex(parentMenuCd);

		if (insertRowIndex === -1) {
			insertRowIndex = focusedRowIndex + 1;
		}
		//var menuCd = scwin.getInsertRowMenuCd(parentMenuCd, menuLevel, true);
		
		var insertJson = [{
			"DVC_ID" : dma_SEQ_BMS_DVC_INFO_0.get("SEQ"),
			"PARENT_ID" : parentMenuCd,
			"LEVEL" : menuLevel,
			"VHC_ID" : dlt_BMS_VHC_MST.getCellData(VD0100G0.getFocusedRowIndex(), "VHC_ID")
			//"SORT" : sortOrder
		}];
		
		dlt_BMS_DVC_INFO.insertJSON(insertRowIndex, insertJson);
		//dlt_BMS_DVC_INFO.setCellData(insertRowIndex, "USE_YN", 'Y');
		VD0100G1.setFocusedCell(insertRowIndex, "DVC_ID", true);

	};
	
	/**
	 * 같은 레벨의 노트에 메뉴를 추가한다.
	 */
	scwin.equalLevel = function() {
		var focusedRowIndex = VD0100G1.getFocusedRealRowIndex();
		var parentMenuCd = dlt_BMS_DVC_INFO.getCellData(focusedRowIndex, "PARENT_ID");
		var rowIndexArr = dlt_BMS_DVC_INFO.getMatchedIndex("PARENT_ID", parentMenuCd, true, 0, dlt_BMS_DVC_INFO.getTotalRow());
		var menuLevel = WebSquare.util.getNumber(dlt_BMS_DVC_INFO.getCellData(rowIndexArr[rowIndexArr.length - 1], "LEVEL"));
		//var sortOrder = WebSquare.util.getNumber(dlt_BMS_DVC_INFO.getCellData(rowIndexArr[rowIndexArr.length - 1], "SORT")) + 1;
		var insertRowIndex = scwin.getInsertRowIndex(parentMenuCd);
		//var menuCd = scwin.getInsertRowMenuCd(parentMenuCd, menuLevel, false);
		
		var insertJson = [{
			"DVC_ID" : dma_SEQ_BMS_DVC_INFO_0.get("SEQ"),
			"PARENT_ID" : parentMenuCd,
			"LEVEL" : menuLevel,
			"VHC_ID" : dlt_BMS_VHC_MST.getCellData(VD0100G0.getFocusedRowIndex(), "VHC_ID")
			//"SORT" : sortOrder
		}];
		
		dlt_BMS_DVC_INFO.insertJSON(insertRowIndex, insertJson);
		//dlt_BMS_DVC_INFO.setCellData(insertRowIndex, "USE_YN", 'Y');
		VD0100G1.setFocusedCell(insertRowIndex, "DVC_ID", true);
	};
	
	/**
	 * 새로 추가되는 메뉴의 메뉴 코드를 반환한다. ( LEVEL 3일 경우 하위메뉴 생성 )
	 */
	scwin.getInsertRowMenuCd = function(parentMenuCd, menuLevel, isBottom) {
	
		var prefix = 'MN00';
		var menuCdArr = dlt_BMS_DVC_INFO.getMatchedColumnData("PARENT_ID", parentMenuCd, "DVC_ID", true, 0, dlt_BMS_DVC_INFO.getTotalRow());
		if (menuCdArr.length > 0) {
			var lastMenuCd = menuCdArr[menuCdArr.length - 1];
			var nodeIndex = prefix.length+(menuLevel - 1) * 2;
			var preNodeCd = lastMenuCd.substr(nodeIndex, 2);
			var newNodeCd = com.fillZero(WebSquare.util.getNumber(preNodeCd) + 1, 2);
			return lastMenuCd.substr(0, nodeIndex) + lastMenuCd.substr(nodeIndex).replace(preNodeCd, newNodeCd);
		} else {
			if (isBottom) {
				var nodeIndex = prefix.length + parentMenuCd.indexOf("00");
				return parentMenuCd.substr(0, nodeIndex) + parentMenuCd.substr(nodeIndex).replace("00", "01");
			} else {
				var nodeIndex = prefix.length + parentMenuCd.indexOf("00");
				var preNodeCd = parentMenuCd.substr(nodeIndex, 2);
				var newNodeCd = com.fillZero(WebSquare.util.getNumber(preNodeCd) + 1, 2);
				return parentMenuCd.substr(0, nodeIndex) + parentMenuCd.substr(nodeIndex).replace(preNodeCd, newNodeCd);
			}
		}
	};

	/**
	 * 새로 추가되는 메뉴로 RowIndex를 반환한다.
	 */
	scwin.getInsertRowIndex = function(parentMenuCd) {
		var rowIndexArr = dlt_BMS_DVC_INFO.getMatchedIndex("PARENT_ID", parentMenuCd, true, 0, dlt_BMS_DVC_INFO.getTotalRow());
		var rowIdx = "";
		
		if (rowIndexArr.length > 0) {
			var lastIndex = rowIndexArr[rowIndexArr.length - 1];
			
			var rowIdx = dlt_BMS_DVC_INFO.getMatchedIndex("PARENT_ID", dlt_BMS_DVC_INFO.getRowJSON(lastIndex).DVC_ID, true, 0, dlt_BMS_DVC_INFO.getTotalRow());
			if(rowIdx == "") {
				rowIdx = dlt_BMS_DVC_INFO.getMatchedIndex("DVC_ID", dlt_BMS_DVC_INFO.getRowJSON(lastIndex).DVC_ID, true, 0, dlt_BMS_DVC_INFO.getTotalRow());
			}
			
			var lastMenuCd = dlt_BMS_DVC_INFO.getCellData(rowIdx[rowIdx.length - 1], "DVC_ID");
			var rowIndex = scwin.getInsertRowIndex(lastMenuCd);
			if (rowIndex !== -1) {
				return rowIndex;
			} else {
				return rowIdx[rowIdx.length - 1] + 1;
			}
		} else {
			return -1;
		}
	};

