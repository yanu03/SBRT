//			ADD: {}, DEL:{}, CNL:{}
//		};
//		com.setSubBtn2(wfm_subBtn, autoOptions, subOptions);
		
		var codeOptions = [ {code : "AREA", compID : "VD0100G0:AREA" },
							{code : "VHC_KIND", compID : "VD0100G0:VHC_KIND"},
							{code : "ROUT_TYPE", compID : "VD0100G0:ROUT_TYPE"},
							{code : "VHC_TYPE", compID : "VD0100G0:VHC_TYPE"},
							{code : "VHC_FUEL", compID : "VD0100G0:VHC_FUEL"},
							{code : "DVC_KIND", compID : "DVC_KIND, VD0100G1:DVC_KIND"},
							{code : "INST_LOC", compID : "INST_LOC, VD0100G1:INST_LOC"}
		 ];
		 
		com.setCommonDtl(codeOptions, function() {
			com.searchGrid(VD0100G0, sub_VD0100G0R0);
		});
						 
		$p.executeSubmission(sub_VD0100SHI0);	
		$p.executeSubmission(sub_VD0100SHI1);	
	};

	scwin.onpageunload = function() {

	};

	//조회 콜백 메소드
	scwin.sub_VD0100G0R0_submitdone = function(e) {
		com.setFocusedCell(VD0100G0, "VHC_ID");
	};
	
	//그리드 저장 후 조회
	scwin.sub_VD0100G1S0_submitdone = function(e) {
		com.saveDone(VD0100G1,sub_VD0100G1R0);
	};
	
	//키값 추가 submission done
	scwin.sub_VD0100G1K0_submitdone = function(e) {
		com.addGridForm(VD0100G1, VD0100F0, dma_SEQ_BMS_DVC_INFO_0, "DVC_ID", "DVC_ID");
		
		//VHC_ID 가져와서 SET
		var focusIndex = VD0100G1.getFocusedRowIndex();
		dlt_BMS_DVC_INFO.setCellData(focusIndex, "VHC_ID", dlt_BMS_VHC_MST.getCellData(VD0100G0.getFocusedRowIndex(), "VHC_ID"));

	};	
	
	scwin.sub_VD0100G0S0_submitdone = function(e) {
		var sCode = com.getResultCode(e);
		if (sCode == gcm.MESSAGE_CODE.STATUS_SAVE) {
			com.saveDone(VD0100G0,sub_VD0100G0R0);
		}
	};
		
		
	scwin.sbx_ChoiceBox_onviewchange = function(info) {
		
		if (info.newValue == "ALL") {
			autoChoiceBox.hide();
			search_input.setValue("");
			search_input.show("");
		} else if (info.newValue == "VHC_ID") {
			search_input.hide();
			autoChoiceBox.setNodeSet("data:dlt_searchitem", "VHC_ID", "VHC_ID");
			autoChoiceBox.show("");
		} else if (info.newValue == "VHC_NO") {
			search_input.hide();
			autoChoiceBox.setNodeSet("data:dlt_searchitem", "VHC_NO", "VHC_NO");
			autoChoiceBox.show("");
		}
	};
	

	scwin.VD0100G0_onrowindexchange = function(row,oldRow) {
		dma_subsearch.set("ORG_ID", dlt_BMS_VHC_MST.getCellData(row, "ORG_ID"))
		var focusOption = {keyMap : dma_subsearch};
		com.changeDualGrid(VD0100G0, VD0100G1, sub_VD0100G1S0, sub_VD0100G1R0, focusOption, row, oldRow);
	};
	
	scwin.sub_VD0100G1R0_submitdone = function(e) {
		com.setFocusedCell(VD0100G1, "DVC_ID");		
	};
	
	//위치좌표 팝업
	scwin.DVC_COORDS_onclick = function(e) {
		var data = { VHC_KIND : dlt_BMS_VHC_MST.getCellData(VD0100G0.getFocusedRowIndex(), "VHC_KIND"), 
					 DVC_ID : dlt_BMS_DVC_INFO.getCellData(VD0100G1.getFocusedRowIndex(), "DVC_ID"),
					 DVC_KIND : dlt_BMS_DVC_INFO.getCellData(VD0100G1.getFocusedRowIndex(), "DVC_KIND"),
					 DVC_COORDS : dlt_BMS_DVC_INFO.getCellData(VD0100G1.getFocusedRowIndex(), "DVC_COORDS"),
				   	 callbackFn : "scwin.searchCOORDSCallback" };
				   	 
		var opt = { id : "FM0100P01", 
					popupName : "시설물 위치 좌표 설정",
					modal : true,  
					width : 1400, height: 450 };
					
		if(data.DVC_KIND != null && data.DVC_KIND != ''){
			com.openPopup("/ui/BMS/VD0100/VD0100P01.xml", opt, data);
		}else if (data.DVC_KIND == null){
			alert('장치 유형을 선택해주세요.');
		}		
	};
	
	//위치 좌표 버튼 콜백함수
