							{code : "VHC_TYPE", compID : "VD0100G0:VHC_TYPE"},
							{code : "VHC_FUEL", compID : "VD0100G0:VHC_FUEL"},
							{code : "DVC_KIND", compID : "DVC_KIND, VD0100G1:DVC_KIND"},
