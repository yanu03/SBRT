   scwin.onpageload = function() {
      var codeOptions = [ {code : "ROUT_TYPE", compID : "ROUT_TYPE, SI0401G0:ROUT_TYPE" },
                     {code : "WAY_DIV", compID : "WAY_DIV, USER_WAY_DIV, SI0401G0:WAY_DIV" },
                     {code : "JIT_DSPCH_YN", compID : "JIT_DSPCH_YN, SI0401G0:JIT_DSPCH_YN" },
                     {code : "SHAPE_DIV", compID : "SHAPE_DIV, SI0401G0:SHAPE_DIV" },
                     {code : "AREA", compID : "AREA, SI0401G1:AREA" },
                     {code : "ROUT_TYPE", compID : "ROUT_TYPE, SI0401G0:ROUT_TYPE" },
                     {code : "ROUT_DIV", compID : "ROUT_DIV" }
                  ];      
   
      com.setCommonDtl(codeOptions, function() {
         com.searchGrid(SI0401G0, sub_SI0401G0R0);
      });   
      
      com.executeSubmission(sub_SI0401SHI0);
      
      var autoOptions = {
         Main : { grid : SI0401G0, frm : SI0401F0, srchSbm : sub_SI0401G0R0, savSbm : sub_SI0401G0S0
            , keySbm : sub_SI0401G0K0, srchGrp : pgm_header, keyColumn : "ROUT_ID", focusColumn : "ROUT_NM", name : "노선 목록"},
         Sub1 : { grid : SI0401G1, srchSbm : sub_SI0401G1R0, savSbm : sub_SI0401G1S0, name :  "관련운수사"}
       };
       
      com.setMainBtn2(wfm_mainBtn, gcm.DISP_TYPE.DUAL_GRID_FORM, autoOptions);

      var subOptions = {
         ADD: scwin.btn_add2_onclick, DEL:{}, CNL:{}
      };
      
      com.setSubBtn2(wfm_subBtn, autoOptions, subOptions);
      
      //SI0401G0.mergeCells("byrestriccol");
   };
   
   scwin.onpageunload = function() {
      
   };
   
   scwin.sub_SI0401G0R0_submitdone = function(e) {
      com.setFocusedCell(SI0401G0, "ROUT_ID");   
   };
   
   scwin.sub_SI0401G0K0_submitdone = function(e) {
      com.addGridForm(SI0401G0, SI0401F0, dma_SEQ_BMS_ROUT_MST_0, "ROUT_ID", "ROUT_NM");
   };
   
   scwin.sub_SI0401G0S0_submitdone = function(e) {
      var sCode = com.getResultCode(e);
      if (sCode == gcm.MESSAGE_CODE.STATUS_SAVE) {
         com.saveDone(SI0401G0,sub_SI0401G0R0);
      }
   };
   
   scwin.sbx_ChoiceBox_onviewchange = function(info) {
      if (info.newValue == "ALL") {
         autoChoiceBox.hide();
         search_input.setValue("");
         search_input.show("");
      } 
      else if (info.newValue == "ROUT_ID") {
         search_input.hide();
         autoChoiceBox.setNodeSet("data:dlt_searchitem", "ROUT_ID", "ROUT_ID");
         autoChoiceBox.show("");
      }
      else if (info.newValue == "ROUT_NM") {
         search_input.hide();
         autoChoiceBox.setNodeSet("data:dlt_searchitem", "ROUT_NM", "ROUT_NM");
         autoChoiceBox.show("");
      } else if (info.newValue == "REP_ROUT_NM") {
         search_input.hide();
         autoChoiceBox.setNodeSet("data:dlt_searchitem", "REP_ROUT_NM", "REP_ROUT_NM");
         autoChoiceBox.show("");
      }      
   };
   
   scwin.SI0401G0_onrowindexchange = function(row,oldRow) {
      var focusOption = {keyMap : dma_param_ROUTEID};
      com.changeDualGrid(SI0401G0, SI0401G1, sub_SI0401G1S0, sub_SI0401G1R0, focusOption, row, oldRow);
   };
   
   //서브 그리드 추가 클릭 이벤트
   scwin.btn_add2_onclick = function() {
   
      var focusRowIdx = scwin.idxRow;
      if (focusRowIdx == -1) {
         com.alert("노선 목록 선택후 관련 운수사를 등록하실 수 있습니다.");
      } else {
         var rowStatus = SI0401G0.getRowStatus(focusRowIdx);
         if (rowStatus == "C") {
            com.alert("노선 등록 후에 사용 가능합니다.");
         } else {
            var val_COMP_ID = dlt_BMS_TRANSCOMP_MST.getColData("COMP_ID");
            dma_param_ROUTEID.set("COMP_IDS", val_COMP_ID);
            var param = dma_param_ROUTEID.get("ROUT_ID");
            if (param != "") {
               var data = { data : dma_param_ROUTEID.getJSON(), callbackFn : "scwin.insertSubGrid" };
               var options = { id : "SI0401P01",
                           popupName : "운수사 목록", 
                           modal : true};
               com.openPopup("/ui/BMS/SI0401/SI0401P01.xml", options, data);
            }
         }
      }      
   };
   
   //서브 그리드 추가 콜백함수
   //팝업창의 data를 서브 그리드에 추가
   scwin.insertSubGrid = function(obj) {
      var dLength = obj.length;
      for (i = 0; i < dLength; i++) {
         var data = dlt_BMS_TRANSCOMP_MST.getMatchedIndex("COMP_ID", obj[i].COMP_ID, true);
         if (data.length == 0) {
            obj[i].ROUT_ID = dma_param_ROUTEID.get("ROUT_ID");
            obj[i].chk = "0";
            var idx = dlt_BMS_TRANSCOMP_MST.insertRow();
            dlt_BMS_TRANSCOMP_MST.setRowJSON(idx, obj[i], true);
         }
      }
   };
   
   
   scwin.REP_ROUT_NM_SEARCH_onclick = function(e) {
      var data = {callbackFn : "scwin.REP_ROUT_NM_SEARCH_callback" };
      var options = { id : "SI0401P02",
                  popupName : "대표노선 목록", 
                  modal : true};
      com.openPopup("/cm/common/popUpRepRoutList.xml", options, data);
   };
   


	scwin.WAY_DIV_onviewchange = function(info) {
		
		if ((info.oldValue == "WD002") && (info.newValue == "WD001") || (info.oldValue == "WD001") && (info.newValue == "WD002")) {
			var stSttnId = dlt_BMS_ROUT_MST.getCellData(SI0401G0.getFocusedRowIndex(), "ST_STTN_ID");
			var stSttnNm = dlt_BMS_ROUT_MST.getCellData(SI0401G0.getFocusedRowIndex(), "ST_STTN_NM");
			var stSttnEnm = dlt_BMS_ROUT_MST.getCellData(SI0401G0.getFocusedRowIndex(), "ST_STTN_ENM");
			var edSttnId = dlt_BMS_ROUT_MST.getCellData(SI0401G0.getFocusedRowIndex(), "ED_STTN_ID");
			var edSttnNm = dlt_BMS_ROUT_MST.getCellData(SI0401G0.getFocusedRowIndex(), "ED_STTN_NM");
			var edSttnEnm = dlt_BMS_ROUT_MST.getCellData(SI0401G0.getFocusedRowIndex(), "ED_STTN_ENM");
			dlt_BMS_ROUT_MST.setCellData(SI0401G0.getFocusedRowIndex(), "ST_STTN_ID", edSttnId);
			dlt_BMS_ROUT_MST.setCellData(SI0401G0.getFocusedRowIndex(), "ST_STTN_NM", edSttnNm);
			dlt_BMS_ROUT_MST.setCellData(SI0401G0.getFocusedRowIndex(), "ST_STTN_ENM", edSttnEnm);
			dlt_BMS_ROUT_MST.setCellData(SI0401G0.getFocusedRowIndex(), "ED_STTN_ID", stSttnId);
			dlt_BMS_ROUT_MST.setCellData(SI0401G0.getFocusedRowIndex(), "ED_STTN_NM", stSttnNm);
			dlt_BMS_ROUT_MST.setCellData(SI0401G0.getFocusedRowIndex(), "ED_STTN_ENM", stSttnEnm);
		}
	}
	
	scwin.REP_ROUT_NM_SEARCH_callback = function(obj) {

		var wayDiv = dlt_BMS_ROUT_MST.getCellData(SI0401G0.getFocusedRowIndex(), "WAY_DIV");
		var stSttnId =obj[0].ST_STTN_ID;
		var stSttnNm =obj[0].ST_STTN_NM;
		var stSttnEnm =obj[0].ST_STTN_ENM;
		var edSttnId =obj[0].ED_STTN_ID;
		var edSttnNm =obj[0].ED_STTN_NM;
		var edSttnEnm =obj[0].ED_STTN_ENM;
		
		if(wayDiv == "WD001"){
			dlt_BMS_ROUT_MST.setCellData(SI0401G0.getFocusedRowIndex(), "ST_STTN_ID", stSttnId);
			dlt_BMS_ROUT_MST.setCellData(SI0401G0.getFocusedRowIndex(), "ST_STTN_NM", stSttnNm);
			dlt_BMS_ROUT_MST.setCellData(SI0401G0.getFocusedRowIndex(), "ST_STTN_ENM", stSttnEnm);
			dlt_BMS_ROUT_MST.setCellData(SI0401G0.getFocusedRowIndex(), "ED_STTN_ID", edSttnId);
			dlt_BMS_ROUT_MST.setCellData(SI0401G0.getFocusedRowIndex(), "ED_STTN_NM", edSttnNm);
			dlt_BMS_ROUT_MST.setCellData(SI0401G0.getFocusedRowIndex(), "ED_STTN_ENM", edSttnEnm);
		}
		else {
			dlt_BMS_ROUT_MST.setCellData(SI0401G0.getFocusedRowIndex(), "ST_STTN_ID", edSttnId);
			dlt_BMS_ROUT_MST.setCellData(SI0401G0.getFocusedRowIndex(), "ST_STTN_NM", edSttnNm);
			dlt_BMS_ROUT_MST.setCellData(SI0401G0.getFocusedRowIndex(), "ST_STTN_ENM", edSttnEnm);
			dlt_BMS_ROUT_MST.setCellData(SI0401G0.getFocusedRowIndex(), "ED_STTN_ID", stSttnId);
			dlt_BMS_ROUT_MST.setCellData(SI0401G0.getFocusedRowIndex(), "ED_STTN_NM", stSttnNm);
			dlt_BMS_ROUT_MST.setCellData(SI0401G0.getFocusedRowIndex(), "ED_STTN_ENM", stSttnEnm);
		}
		
		dlt_BMS_ROUT_MST.setCellData(SI0401G0.getFocusedRowIndex(), "REP_ROUT_ID", obj[0].REP_ROUT_ID);
		dlt_BMS_ROUT_MST.setCellData(SI0401G0.getFocusedRowIndex(), "REP_ROUT_NM", obj[0].REP_ROUT_NM);
		dlt_BMS_ROUT_MST.setCellData(SI0401G0.getFocusedRowIndex(), "ROUT_TYPE", obj[0].ROUT_TYPE);
		dlt_BMS_ROUT_MST.setCellData(SI0401G0.getFocusedRowIndex(), "ROUT_DIV", obj[0].ROUT_DIV);
		dlt_BMS_ROUT_MST.setCellData(SI0401G0.getFocusedRowIndex(), "HOLI_YN", obj[0].HOLI_YN);

		//REP_ROUT_NM.setValue(obj[0].REP_ROUT_NM);
	};
   
