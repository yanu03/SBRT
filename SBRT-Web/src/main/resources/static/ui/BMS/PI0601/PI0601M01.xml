	scwin.upload0_onInputValueChange = function(returnValue){
	
		var fileNameIndex = returnValue.lastIndexOf('\\');
		var real_fileName = returnValue.substring(fileNameIndex+1, returnValue.length);
		
		dlt_BIT_VDO_INFO.setCellData(PI0601G0.getFocusedRowIndex(), "FILE_NM", real_fileName);
		
		btn_upload.submit();
		
	}
	
	scwin.upload0_ondone = function(retXml) {
		var fileType = dlt_BIT_VDO_INFO.getCellData(PI0601G0.getFocusedRowIndex(), "FILE_TYPE");
		var real_fileName = dlt_BIT_VDO_INFO.getCellData(PI0601G0.getFocusedRowIndex(), "FILE_NM");
		var src = "/fileUpload/up/" + real_fileName; 
		
		if (fileType == "FT001"){
			var obj_video = document.getElementById("video1");
			obj_video.addEventListener('loadedmetadata', function() {
				var duration = parseInt(obj_video.duration);
				dlt_BIT_VDO_INFO.setCellData(PI0601G0.getFocusedRowIndex(), "PLAY_TM", duration);
			});			
			$p.$("#video1").attr("src", src);
		} else if (fileType == "FT002") {
			$p.$("#image1").attr("src", src);
		}			
		
	};
	

	scwin.PI0601G0_onrowindexchange = function(row,oldRow) {
				
		var fileType = dlt_BIT_VDO_INFO.getCellData(PI0601G0.getFocusedRowIndex(), "FILE_TYPE");
		if (fileType == "FT001"){
			IMG_PLAY_TM.setDisabled(true);
			$p.$("#image1").hide();
			$p.$("#video1").show();
			var path = dlt_BIT_VDO_INFO.getCellData(PI0601G0.getFocusedRowIndex(), "FILE_PATH");
			$p.$("#video1").attr("src", path);	
			
		} else if (fileType == "FT002") {
			IMG_PLAY_TM.setDisabled(false);		
			$p.$("#image1").show();
			$p.$("#video1").hide();						
			var path = dlt_BIT_VDO_INFO.getCellData(PI0601G0.getFocusedRowIndex(), "FILE_PATH");
			$p.$("#image1").attr("src", path);				
		} else {
			$p.$("#image1").hide();
			$p.$("#video1").hide();				
		}

	};
	
	scwin.FILE_TYPE_onchange = function(info) {
		var fileType = FILE_TYPE.getValue();
		if (fileType == "FT001") {
			IMG_PLAY_TM.setDisabled(true);
			$p.$("#image1").hide();
			$p.$("#video1").show();
			var path = dlt_BIT_VDO_INFO.getCellData(PI0601G0.getFocusedRowIndex(), "FILE_PATH");
			$p.$("#video1").attr("src", path);	
		} 
		else if (fileType == "FT002") {
			IMG_PLAY_TM.setDisabled(false);					
			$p.$("#image1").show();
			$p.$("#video1").hide();	
			var path = dlt_BIT_VDO_INFO.getCellData(PI0601G0.getFocusedRowIndex(), "FILE_PATH");
			$p.$("#image1").attr("src", path);	
		} else {
			$p.$("#image1").hide();
			$p.$("#video1").hide();					
		}
	};
	
	scwin.sbx_ChoiceBox_onviewchange = function(info) {
		if (info.newValue == "ALL") {
			acb_autoChoiceBox.hide();
			search_input.setValue("");
			search_input.show("");
		} 
		else if (info.newValue == "VDO_ID") {
			search_input.hide();
			acb_autoChoiceBox.setNodeSet("data:dlt_searchitem", "VDO_ID", "VDO_ID");
			acb_autoChoiceBox.show("");
		}
		else if (info.newValue == "VDO_NM") {
			search_input.hide();
			acb_autoChoiceBox.setNodeSet("data:dlt_searchitem", "VDO_NM", "VDO_NM");
			acb_autoChoiceBox.show("");
		}
	};
