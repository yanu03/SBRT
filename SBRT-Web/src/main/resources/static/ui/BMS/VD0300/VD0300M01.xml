			Sub3 : { grid : VD0300G3, srchSbm : sub_VD0300G3R0, name :  "장애발생 이력"}
		 };
		
		var mainOptions = {
			ADD : scwin.btn_add_onclick
		};
		 
		com.setMainBtn2(wfm_mainBtn, gcm.DISP_TYPE.DUAL_GRID3, autoOptions, mainOptions);
		
		var codeOptions = [ {code : "AREA", compID : "VD0300G0:AREA" },
							{code : "VHC_KIND", compID : "VD0300G0:VHC_KIND"},
							{code : "ROUT_TYPE", compID : "VD0300G0:ROUT_TYPE"},
							{code : "VHC_TYPE", compID : "VD0300G0:VHC_TYPE"},
							{code : "VHC_FUEL", compID : "VD0300G0:VHC_FUEL"},
							{code : "DVC_KIND", compID : "VD0300G1:DVC_KIND"},
							{code : "INST_LOC", compID : "VD0300G1:INST_LOC"},
							{code : "WORK_TYPE", compID : "VD0300G2:WORK_TYPE"}
		 ];
		 
		com.setCommonDtl(codeOptions, function() {
			com.searchGrid(VD0300G0, sub_VD0300G0R0);
		});
						 
		$p.executeSubmission(sub_VD0300SHI0);	
		$p.executeSubmission(sub_VD0300SHI1);	
	};

	scwin.onpageunload = function() {

	};

	//조회 콜백 메소드
	scwin.sub_VD0300G0R0_submitdone = function(e) {
		com.setFocusedCell(VD0300G0, "VHC_ID");
	};
	
//	scwin.sub_VD0300G1S0_submitdone = function(e) {
//		com.saveDone(VD0300G1,sub_VD0300G1R0);
//	};
	
	scwin.sub_VD0300G0S0_submitdone = function(e) {
		var sCode = com.getResultCode(e);
		if (sCode == gcm.MESSAGE_CODE.STATUS_SAVE) {
			com.saveDone(VD0300G0,sub_VD0300G0R0);
		}
	};
		
		
	scwin.sbx_ChoiceBox_onviewchange = function(info) {
		if (info.newValue == "ALL") {
			autoChoiceBox.hide();
			search_input.setValue("");
			search_input.show("");
		} else if (info.newValue == "VHC_ID") {
			search_input.hide();
			autoChoiceBox.setNodeSet("data:dlt_searchitem", "VHC_ID", "VHC_ID");
			autoChoiceBox.show("");
		} else if (info.newValue == "VHC_NO") {
			search_input.hide();
			autoChoiceBox.setNodeSet("data:dlt_searchitem", "VHC_NO", "VHC_NO");
			autoChoiceBox.show("");
		}
	};
	

	scwin.VD0300G0_onrowindexchange = function(row,oldRow) {
		dma_subsearch.set("ORG_ID", dlt_BMS_VHC_MST.getCellData(row, "ORG_ID"))
		var focusOption = {keyMap : dma_subsearch};
		com.changeDualGrid(VD0300G0, VD0300G1, sub_VD0300G1S0, sub_VD0300G1R0, focusOption, row, oldRow);
	};
	
	scwin.sub_VD0300G1R0_submitdone = function(e) {
		com.setFocusedCell(VD0300G1, "DVC_ID");		
	};
	

	scwin.VD0300G1_onrowindexchange = function(row,oldRow) {
		var focusOption = {keyMap : dma_subsearch2};
		com.changeDualGrid(VD0300G1, VD0300G2, sub_VD0300G2S0, sub_VD0300G2R0, focusOption, row, oldRow);
		
		//장애발생 이력 조회
		//전송일시 데이터 안나옴 쿼리문 고쳐야할듯		
		var mngID = dlt_BMS_DVC_INFO.getCellData(row, "MNG_ID");		
		dma_subsearch3.set("MNG_ID", mngID);
		com.executeSubmission(sub_VD0300G3R0);
						
		
	};
	
	scwin.VD0300T0_onchange = function(tabID,idx,userTabID) {
		//장애발생 이력으로 변경시 setCode (임시)
		var codeOption = [{code : "DVC_COND", compID : "VD0300G3:DVC_COND"}];
		com.setCommonDtl(codeOption);
	};
	
	scwin.btn_add_onclick = function() {
		var dvcID = dma_subsearch2.get("DVC_ID");
		com.addGrid2(VD0300G2, "APLY_DATE");
		dlt_BMS_DVC_HIS.setCellData(VD0300G2.getFocusedRowIndex(), "DVC_ID", dvcID);
	}
	
	scwin.sub_VD0300G2S0_submitdone = function(e) {
		com.saveDone(VD0300G2,sub_VD0300G2R0);		
	};
