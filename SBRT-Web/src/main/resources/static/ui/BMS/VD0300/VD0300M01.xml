	scwin.onpageload = function() {
		var autoOptions = {
			Main : { grid : VD0300G0, srchSbm : sub_VD0300G0R0, srchGrp : pgm_header, keyColumn : "VHC_ID", focusColumn : "VHC_NO", name :  "차량 목록"},
