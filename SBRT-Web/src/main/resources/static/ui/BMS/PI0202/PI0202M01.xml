		<script type="text/javascript" lazy="false"><![CDATA[
	var isSoundAdd;

	var curRoutId;
	var curPoint;
	scwin.onpageload = function() {

		var codeOptions = [{code : "NODE_TYPE", compID : "PI0202G1:NODE_TYPE"},{code : "WAY_DIV", compID : "PI0202G0:WAY_DIV"}];
		com.setCommonDtl(codeOptions, function() {
			com.executeSubmission(sub_PI0202G0R0);
		});	
		
		var autoOptions = {
			Main : { grid : PI0202G0, srchSbm : sub_PI0202G0R0, srchGrp : pgm_header, keyColumn : "ROUT_ID", focusColumn : "ROUT_ID", name : "노선 목록"},
			Sub1 : { grid : PI0202G1, srchSbm : sub_PI0202G1R0, srchGrp : pgm_subList, savSbm : sub_PI0202G1S0, name :  "노선 지점"}
		 };
		 
		var mainOptions = {
			ADD:{disp:"hide"},
			DEL:{disp:"hide"},
			CNL:scwin.btn_cancel_onclick,
			SAVE:scwin.btn_save_onclick
		};
		
		com.setMainBtn2(wfm_mainBtn, gcm.DISP_TYPE.DUAL_GRID2, autoOptions, mainOptions);
	
		var subOptions = {
			DEL:scwin.btn_sub_del
			, SEARCH: scwin.btn_sub_move
		};
		
		com.setSubBtn2(wfm_subBtn, autoOptions, subOptions);
		
		var subOptions2 = {
			SAVE:[{cbFnc:scwin.addSoundClick,nm:"음성편성 추가",class:"addPath"},{cbFnc:scwin.btn_sub_del,nm:"선택삭제",class:"del"}
			]
		};

		com.setSubBtn2(wfm_subBtn2, autoOptions, subOptions2);
		
		PI0202G0.mergeCells("byrestriccol");
		

		routMap.initMap("map_PI0202", {width:"100%"
			, height:"100%"
			, onClick: scwin.onClickMap
			, nodeChangeCb: scwin.nodeChangeCb
			, draggable: true
			, isChange: true
			, isSound: true
		});
		
		com.executeSubmission(sub_PI0202SHI0);	
	};
	
	scwin.dateDisFormatter = function(data){
		if (data == "") return "";
		return data.substring(0,4) + "-" + data.substring(4,6) + "-" + data.substring(6,8) + " " + data.substring(8,10) + ":" + data.substring(10,12);
	}

	scwin.onpageunload = function() {
		
	};
	
	scwin.sub_PI0202G0R0_submitdone = function(e) {
		scwin.releaseGeoBtn();
		com.setFocusedCell(PI0202G0, "ROUT_ID");	
	};
	
	scwin.sbx_ChoiceBox_onviewchange = function(info) {
		if (info.newValue == "ALL") {
			autoChoiceBox.hide();
			search_input.setValue("");
			search_input.show("");
		} 
		else if (info.newValue == "ROUT_ID") {
			search_input.hide();
			autoChoiceBox.setNodeSet("data:dlt_searchitem", "ROUT_ID", "ROUT_ID");
			autoChoiceBox.show("");
		}
		else if (info.newValue == "ROUT_NM") {
			search_input.hide();
			autoChoiceBox.setNodeSet("data:dlt_searchitem", "ROUT_NM", "ROUT_NM");
			autoChoiceBox.show("");
		} 
		else if (info.newValue == "REP_ROUT_NM") {
			search_input.hide();
			autoChoiceBox.setNodeSet("data:dlt_searchitem", "REP_ROUT_NM", "REP_ROUT_NM");
			autoChoiceBox.show("");
		}		
	};
	
	
	scwin.PI0202G0_onrowindexchange = function(row, oldRow) {
		routMap.initMapInfo("map_PI0202");
		curRoutId = dlt_BMS_ROUT_MST.getCellData(row,"ROUT_ID");
		var focusOption = {keyMap : dma_sub_search};
		com.changeDualGrid(PI0202G0, PI0202G1, sub_PI0202G1S0, sub_PI0202G1R0, focusOption, row, oldRow);
       
	};	
	scwin.PI0202G1_onrowindexchange = function(row,oldRow) {
		routMap.focusNode("map_PI0202",PI0202G1,row);
	};
	
	scwin.sub_PI0202G1R0_submitdone = function(e) {
		if(dlt_BMS_ROUT_NODE_CMPSTN.getTotalRow() == 0){
			routMap.initDisplay("map_PI0202");
		}
		else {
			//com.setFocusedCell(PI0202G1, "NODE_ID");
			if(PI0202G1.getFocusedRowIndex() == -1 ){
				PI0202G1.setFocusedCell( 0, "NODE_ID");
			}
			else {
				PI0202G1.setFocusedCell( PI0202G1.getFocusedRowIndex(), "NODE_ID");
			}
		}
		scwin.btn_sub_move();
		//routMap.drawRoute("map_PI0202", PI0202G1, -1, true);
	};
	
	scwin.sub_PI0202G1S0_submitdone = function(e) {
		var sCode = com.getResultCode(e);
		if (sCode == gcm.MESSAGE_CODE.STATUS_SAVE) {
			scwin.releaseGeoBtn();
			routMap.initMapInfo("map_PI0202");
			com.saveDone(PI0202G1,sub_PI0202G1R0);
		}
	}
	
	scwin.select_sound = function(isAdd) {
		
		var val_ORGA_ID = dlt_BMS_ROUT_NODE_CMPSTN.getColData("ORGA_ID");
		dma_sub_search.set("ORGA_IDS", val_ORGA_ID);
		var param = dma_sub_search.get("ROUT_ID");
		if (param != "") {
			var data = null;
			if(isAdd){
				data = { data : dma_sub_search.getJSON(), callbackFn : "scwin.add_sound_done" };
			}
			else{
				data = { data : dma_sub_search.getJSON(), callbackFn : "scwin.select_sound_done" };
			}
			var options = { id : "PI0202P01",
							popupName : "음성편성선택", 
							modal : true};
			com.openPopup("/ui/BMS/PI0202/PI0202P01.xml", options, data);
		}
	}
	
	scwin.select_sound_done = function(obj){
		if (typeof obj === "undefined") {
			dlt_BMS_ROUT_NODE_CMPSTN.setCellData(PI0202G1.getFocusedRowIndex(),"NODE_TYPE", oldNodeType);
		}
		else {
			dlt_BMS_ROUT_NODE_CMPSTN.setCellData(PI0202G1.getFocusedRowIndex(),"NODE_TYPE", routMap.NODE_TYPE.SOUND);
			scwin.mappingNode("map_PI0202", PI0202G1, SI0202G1.getFocusedRowIndex(), obj[0], true);
		}
	}

	scwin.add_sound_done = function(obj){
		 
		if (typeof obj === "undefined") {

		}
		else {
			var idx = routMap.addSoundByClick("map_PI0202", PI0202G1,curRoutId,curPoint);
			scwin.mappingNode("map_PI0202", PI0202G1, idx, obj[0], true);
		}
	}
	scwin.mappingNode = function(mapId, grid, index, data, draggable){
		
		if(data==null)return;

		com.getGridViewDataList(grid).setCellData(index, "ORGA_ID",data.ORGA_ID);
		com.getGridViewDataList(grid).setCellData(index, "NODE_NM",data.ORGA_NM);
		com.getGridViewDataList(grid).setCellData(index, "ALL_PLAY_TM",data.ALL_PLAY_TM);
		routMap.drawSound(mapId, grid, index, draggable);
	}

	scwin.btn_cancel_onclick = function() {
		var idx = PI0202G1.getModifiedIndex().length;
		
		if (idx > 0) {
			var str =com.strModifiedCnt(PI0202G1) + "의 저장하지 않은 데이터가 있습니다. 취소 하시겠습니까?";
			com.confirm(str, function(rtn) {
				if (rtn) {
					com.clearGrid(PI0202G1);
					com.searchGrid(PI0202G1, sub_PI0202G1R0);
				}
			});
		}

	};
	
	scwin.btn_save_onclick = function() {
		
		
		com.setSerialNumberToData(PI0202G1, "NODE_SN", "OLD_NODE_SN");
		
		var idx = PI0202G1.getModifiedIndex().length;
	
		if (idx == 0) {
			if(	(typeof str == "undefined") || (str.trim() == ""))
				str = "저장할 데이터가 없습니다";
			com.alert(str);
			return false;
		}
	
		if (com.validateGridView(PI0202G1)) {
			if (idx > 0) {
				var yesno_str = com.strModifiedCnt(PI0202G1) + "의 데이터가 저장됩니다. \n 진행하시겠습니까?";
				com.confirm(yesno_str, function(rtn) {
					if (rtn) {				
						com.saveData(PI0202G1,null,sub_PI0202G1S0);
					}
				});
			}
		}

	};
	
	//맵 클릭 이벤트
	scwin.onClickMap = function(e) {
		
		curPoint = e;
		if(isSoundAdd) {
			scwin.select_sound(true);
		}
	}

	scwin.nodeChangeCb = function(idx) {
		if(isPathAdd || isVertexAdd || isSttnAdd || isCrossAdd) return;
		PI0202G1.setFocusedCell(idx,"NODE_ID");
		scwin.select_sound(false);
	
	
	
	}

	scwin.btn_sub_del = function() {
		
		var nodeType = dlt_BMS_ROUT_NODE_CMPSTN.getCellData(PI0202G1.getFocusedRowIndex(), "NODE_TYPE");
		if(nodeType != routMap.NODE_TYPE.SOUND)com.alert("음성노드만 삭제 가능합니다.");
		else
		com.delGrid(PI0202G1);
	};
	
	scwin.btn_sub_move = function() {
		if(com.isEmpty(dma_sub_search.get("CONTENT"))==false){
			com.gridMove(PI0202G1, "NODE_ID,NODE_NM", dma_sub_search.get("CONTENT"), PI0202G1.getFocusedRowIndex());
		}
	};
	
	scwin.addSoundClick = function(){
		if(isSoundAdd) {
			isSoundAdd = false;
		} else {;
			isSoundAdd = true;
		}
		scwin.changeGeoBtn();
	}


	
	scwin.releaseGeoBtn = function(){
		isSoundAdd = false;

		scwin.changeGeoBtn();
	}
	
	scwin.changeGeoBtn = function(){

		var temp = wfm_subBtn2.getWindow().btn_sub_generator;
		
		if(isSoundAdd) {
			temp.getChild(0,"btn_sub").setValue("음성편성 종료");
			temp.getChild(0,"btn_sub").addClass("end");
		}
		else {
			temp.getChild(0,"btn_sub").setValue("음성편성 추가");
			temp.getChild(0,"btn_sub").removeClass("end");
		}
	
	}
	]]></script>
