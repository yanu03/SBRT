	scwin.onpageload = function() {
		var autoOptions = {
			Main : { grid : SI0101G0, srchSbm : sub_SI0101G0R0, savSbm : sub_SI0101G0S0, keySbm : sub_SI0101G0K0 
					,srchGrp : pgm_header, keyColumn : "GRG_ID", focusColumn : "GRG_NM", name :  "차고지 목록"}
		};
		com.setMainBtn2(wfm_mainBtn, gcm.DISP_TYPE.SINGLE_GRID, autoOptions);
		
		var codeOptions = [ {code : "OWNER_TYPE", compID : "SI0101G0:OWNER_TYPE" } ];
		
		com.setCommonDtl(codeOptions, function(){
			com.searchGrid(SI0101G0, sub_SI0101G0R0);
		});			
		
		$p.executeSubmission(sub_SI0101SHI0);

	};

	scwin.onpageunload = function() {

	};

	//validation 처리를 위한 옵션
	//mandatory : 필수 체크, valFunc : 콜백 함수
	//var valInfo = [ { id : "GRG_NM", mandatory : true},
	//				{ id : "OWNER_TYPE", mandatory : true}
	//			  ];
					
	
	scwin.sub_SI0101G0R0_submitdone = function(e) {
		var sCode = com.getResultCode(e);
		if (sCode == gcm.MESSAGE_CODE.STATUS_SUCCESS) {
			com.setFocusedCell(SI0101G0, "GRG_NM");
		}
	};
	
	/**
	 * 프로그램관리 저장 버튼의 콜백메소드
	 */
	scwin.sub_SI0101G0S0_submitdone = function(e) {
		var sCode = com.getResultCode(e);
		if (sCode == gcm.MESSAGE_CODE.STATUS_SAVE) {
			com.saveDone(SI0101G0,sub_SI0101G0R0);
		}
	};

	scwin.btn_add_onclick = function() {
		$p.executeSubmission(sub_SI0101G0K0);
	};

	scwin.sub_SI0101G0K0_submitdone = function(e) {
		var insertIndex = com.addGrid(SI0101G0,dma_SEQ_BMS_GRG_MST_0,"GRG_ID","GRG_NM");
	};

	scwin.btn_del_onclick = function() {
		com.delGrid(SI0101G0);
	};
	
	scwin.btn_cancel_onclick = function() {
		com.cancelGrid(SI0101G0);
	};
	
	scwin.btn_excel_onclick = function() {
		com.exlGrid(SI0101G0);
	};

	/**	 
	 * 엑셀의 데이터를 그리드뷰로 업로드 한다.
	 * 필요한 옵션이 있다면 options배열에 옵션을 넣는다.
	 */
	scwin.btn_uploadExcel_onclick = function() {
		com.exlUploadGrid(SI0101G0);
	};
	
	
	/**
	 * 그리드 Excel 포맷을 다운로드 하는 기능 ( 업로드용  Excel 포맷 )
	 */
	scwin.btn_excelForm_onclick = function() {
		com.exlFrmGrid(SI0101G0);	
	};
	
	scwin.btn_close_onclick = function() {
		com.closeTab(SI0101G0);
	};

	
	scwin.SI0101G0_oncellclick = function(row, col, colId) {
	
		var gridColumn = SI0101G0.getColumnID(col);
		
		if (gridColumn == "LOCATION") {
				var param = SI0101G0.getDataRow(row).GRG_ID;
				if (param != "") {
					dma_sub_search.set("GRG_ID", param);
					dma_sub_search.set("GRG_NM",SI0101G0.getDataRow(row).GRG_NM);
					
					var data = { data : dma_sub_search.getJSON(), 
								 GPS_X : dlt_BMS_GRG_MST.getCellData(row, "GPS_X"), 
					 			 GPS_Y : dlt_BMS_GRG_MST.getCellData(row, "GPS_Y"),	
								 callbackFn : "scwin.insertPopupData" };
					
					var options = {
						id : "SI0101P01",
						popupName : "위치 지도",
						modal : true
					};
					com.openPopup(("/ui/BMS/SI0101/SI0101P01.xml"), options, data);
			}
		}
	};
	
	scwin.insertPopupData = function(obj) {
		if(obj[0].CENTER_GPS_X != "" && obj[0].CENTER_GPS_Y != "") {
			dlt_BMS_GRG_MST.setCellData(SI0101G0.getFocusedRowIndex(), "GPS_X", obj[0].CENTER_GPS_X);	
			dlt_BMS_GRG_MST.setCellData(SI0101G0.getFocusedRowIndex(), "GPS_Y", obj[0].CENTER_GPS_Y);	
		}
	}
	
	scwin.sbx_ChoiceBox_onviewchange = function(info) {		
		if (info.newValue == "ALL") {
			acb_autoChoiceBox.hide();
			search_input.setValue("");
			search_input.show("");
		} else if (info.newValue == "GRG_ID") {
			search_input.hide();
			acb_autoChoiceBox.setNodeSet("data:dlt_grgSearchItem", "GRG_ID", "GRG_ID");
			acb_autoChoiceBox.show("");
		} else if (info.newValue == "GRG_NM") {
			search_input.hide();
			acb_autoChoiceBox.setNodeSet("data:dlt_grgSearchItem", "GRG_NM", "GRG_NM");
			acb_autoChoiceBox.show("");
		}
	};
	
	scwin.SI0101G0_onrowindexchange = function(row, oldRow) {
		com.changeSingleGrid(SI0101G0, row, oldRow);
	};
