		var temp = wfm_subBtn.getWindow().btn_sub_generator;
		
		if(isEdit) {
			temp.getChild(0,"btn_sub").setValue("편집종료");
			temp.getChild(0,"btn_sub").addClass("end");
		}
		else {
			temp.getChild(0,"btn_sub").setValue("편집시작");
			temp.getChild(0,"btn_sub").removeClass("end");
		}
	}

