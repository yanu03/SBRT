	<script type="text/javascript" lazy="false"><![CDATA[
	scwin.onpageload = function() {
		var autoOptions = {
			Main : { grid : FM0300G0, srchSbm : sub_FM0300G0R0, srchGrp : pgm_header, keyColumn : "STTN_ID", focusColumn : "STTN_NO", name :  "정류장 목록"},
			Sub1 : { grid : FM0300G1, srchSbm : sub_FM0300G1R0, savSbm : sub_FM0300G1S0, keyColumn : "FCLT_ID", name :  "시설물 목록"},
			Sub2 : { grid : FM0300G2, srchSbm : sub_FM0300G2R0, savSbm : sub_FM0300G2S0, name :  "유지보수 이력"},
			Sub3 : { grid : FM0300G3, srchSbm : sub_FM0300G3R0, name :  "장애발생 이력"}
		 };
		
		var mainOptions = {
			ADD : scwin.btn_add_onclick,
			SAVE : scwin.btn_save_onclick,
			DEL : scwin.btn_del_onclick
		};
		 
		com.setMainBtn2(wfm_mainBtn, gcm.DISP_TYPE.DUAL_GRID3, autoOptions, mainOptions);
		
		var codeOptions = [ 
							{code : "FCLT_KIND", compID : "FM0300G1:FCLT_KIND"},
							{code : "INST_LOC", compID : "FM0300G1:INST_LOC"},
							{code : "WORK_TYPE", compID : "FM0300G2:WORK_TYPE"},
							{code : "FCLT_COND", compID : "FM0300G3:FCLT_COND"}
		 ];
		 
		com.setCommonDtl(codeOptions, function() {
			com.searchGrid(FM0300G0, sub_FM0300G0R0);
		});
						 
		$p.executeSubmission(sub_FM0300SHI0);	
		$p.executeSubmission(sub_FM0300SHI1);	
		$p.executeSubmission(sub_FM0300SHI2);	
		
		//$("#FM0300_TAB").attr("style", "width:200px; height:26px;  float:left");
		FM0300G3.hide();
		
		//탭 목록 클릭 이벤트
		$("#FM0300_TAB").find("li").click( function() {
			var li_id = this.id;
			$("#FM0300_TAB").find("li").removeClass("on");
			$("#FM0300_TAB").find("#"+li_id).addClass("on");
			
			if(li_id == "li_FCLT_HIS") {
				FM0300G2.show("");
				FM0300G3.hide();
			}
			else if(li_id == "li_FCLT_COND") {
				FM0300G2.hide();
				FM0300G3.show("");
				//com.setFocusedCell(PI0400G1, "UPD_DTM");
			}
		});
	};

	scwin.onpageunload = function() {

	};

	//조회 콜백 메소드
	scwin.sub_FM0300G0R0_submitdone = function(e) {
		com.setFocusedCell(FM0300G0, "STTN_ID");
	};
	
//	scwin.sub_FM0300G1S0_submitdone = function(e) {
//		com.saveDone(FM0300G1,sub_FM0300G1R0);
//	};
	
	scwin.sub_FM0300G0S0_submitdone = function(e) {
		var sCode = com.getResultCode(e);
		if (sCode == gcm.MESSAGE_CODE.STATUS_SAVE) {
			com.saveDone(FM0300G0,sub_FM0300G0R0);
		}
	};
	
	scwin.sub_FM0300G2S0_submitdone = function(e) {
		var sCode = com.getResultCode(e);
		if (sCode == gcm.MESSAGE_CODE.STATUS_SAVE) {
			com.saveDone(FM0300G2,sub_FM0300G2R0);
		}
	};
	
	scwin.sub_FM0300G3S0_submitdone = function(e) {
		var sCode = com.getResultCode(e);
		if (sCode == gcm.MESSAGE_CODE.STATUS_SAVE) {
			com.saveDone(FM0300G3,sub_FM0300G3R0);
		}
	};
		
		
	scwin.sbx_ChoiceBox_onviewchange = function(info) {
		if (info.newValue == "ALL") {
			autoChoiceBox.hide();
			search_input.setValue("");
			search_input.show("");
		} else if (info.newValue == "STTN_ID") {
			search_input.hide();
			autoChoiceBox.setNodeSet("data:dlt_searchitem", "STTN_ID", "STTN_ID");
			autoChoiceBox.show("");
		} else if (info.newValue == "STTN_NO") {
			search_input.hide();
			autoChoiceBox.setNodeSet("data:dlt_searchitem", "STTN_NO", "STTN_NO");
			autoChoiceBox.show("");
		}
	};
	

	scwin.FM0300G0_onrowindexchange = function(row,oldRow) {
		
		//dma_subsearch.set("ORG_ID", dlt_BMS_STTN_MST.getCellData(row, "ORG_ID"))
		var focusOption = {keyMap : dma_subsearch};
		com.changeDualGrid(FM0300G0, FM0300G1, sub_FM0300G1S0, sub_FM0300G1R0, focusOption, row, oldRow);
	};
	
	scwin.sub_FM0300G1R0_submitdone = function(e) {
		com.setFocusedCell(FM0300G1, "FCLT_ID");		
	};
	

	scwin.FM0300G1_onrowindexchange = function(row,oldRow) {
		var focusOption = {keyMap : dma_subsearch2};
		com.changeDualGrid(FM0300G1, FM0300G2, sub_FM0300G2S0, sub_FM0300G2R0, focusOption, row, oldRow);
		
		//장애발생 이력 조회
		//전송일시 데이터 안나옴 쿼리문 고쳐야할듯		
		var mngID = dlt_BMS_FCLT_INFO.getCellData(row, "MNG_ID");		
		dma_subsearch3.set("MNG_ID", mngID);
		com.executeSubmission(sub_FM0300G3R0);
						
		
	};
	
	scwin.FM0300T0_onchange = function(tabID,idx,userTabID) {
		//장애발생 이력으로 변경시 setCode (임시)
		var codeOption = [{code : "DVC_COND", compID : "FM0300G3:FCLT_COND"}];
		com.setCommonDtl(codeOption);
	};
	
	scwin.btn_add_onclick = function() {
		var fcltID = dlt_BMS_FCLT_INFO.getCellData(FM0300G1.getFocusedRowIndex(), "FCLT_ID");
		var mngID = dlt_BMS_FCLT_INFO.getCellData(FM0300G1.getFocusedRowIndex(), "MNG_ID");
		var liHisClass = $('#li_FCLT_HIS').attr('class'); 
		var liCondClass = $('#li_FCLT_COND').attr('class'); 
		
		if(liHisClass == 'on'){
			com.addGrid2(FM0300G2, "APLY_DATE");
			dlt_BMS_FCLT_HIS.setCellData(FM0300G2.getFocusedRowIndex(), "FCLT_ID", fcltID);
		}else if(liCondClass == 'on'){
			com.addGrid2(FM0300G3, "SEND_DATE");
			dlt_BMS_FCLT_COND_LOG.setCellData(FM0300G3.getFocusedRowIndex(), "MNG_ID", mngID);
		}
		
	}
	
	scwin.btn_save_onclick = function(){
		var liHisClass = $('#li_FCLT_HIS').attr('class'); 
		var liCondClass = $('#li_FCLT_COND').attr('class'); 
		
		if(liHisClass == 'on'){
			com.saveGrid(FM0300G2, sub_FM0300G2S0);
		}else if(liCondClass == 'on'){
			com.saveGrid(FM0300G3, sub_FM0300G3S0);
		}
		
	}
	
	scwin.btn_del_onclick = function(){
		var liHisClass = $('#li_FCLT_HIS').attr('class'); 
		var liCondClass = $('#li_FCLT_COND').attr('class'); 
		
		if(liHisClass == 'on'){
			com.delGrid(FM0300G2);
		}else if(liCondClass == 'on'){
			com.delGrid(FM0300G3);
		}
	}
	
	scwin.sub_FM0300G2S0_submitdone = function(e) {
		com.saveDone(FM0300G2,sub_FM0300G2R0);		
	};
	
	]]></script>
