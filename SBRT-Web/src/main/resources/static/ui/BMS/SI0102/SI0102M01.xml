	scwin.insertCool = function(cooldinate) {
		
		var row = SI0102G0.getFocusedRowIndex();
		
		dlt_BMS_TRANSCOMP_MST.setCellData(row, "GPS_X", cooldinate.substr(0, 10));
		dlt_BMS_TRANSCOMP_MST.setCellData(row, "GPS_Y", cooldinate.substr(16, 24));
