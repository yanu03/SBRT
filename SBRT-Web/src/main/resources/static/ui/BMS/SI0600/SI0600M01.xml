			search_input.hide();
			acb_autoChoiceBox.setNodeSet("data:dlt_EMER_CLSFCTN_MST", "CLSFCTN_NM", "CLSFCTN_NM");
			acb_autoChoiceBox.show("");
		} 
	};]]></script>
