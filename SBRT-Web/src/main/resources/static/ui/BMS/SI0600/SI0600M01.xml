	}
	
	scwin.btn_close_onclick = function() {
		
	};
	
	//here
	scwin.btn_save2_onclick = function() {
		com.executeSubmission(sub_SI0600G2S0);
	}
	
	scwin.btn_add2_onclick = function() {
		var focusRowIdx = SI0600T0.getSelectedIndex();
		if (focusRowIdx == "") {
			com.alert("분류 목록 선택 후 담당자 목록을 등록하실 수 있습니다.");
		} else {
			var rowStatus = dlt_EMER_CLSFCTN_MST.getRowStatus(focusRowIdx);
			if (rowStatus == "C") {
				com.alert("분류 목록 등록 후 사용 가능합니다.");
			} else {
				var val_MEMER_ID = dlt_EMER_MEMBER_INFO.getColData("MEMER_ID");
				dma_param_CLSFCTN.set("MEMER_IDS", val_MEMER_ID);
				var param = dma_param_CLSFCTN.get("CLSFCTN_ID");
				if (param != "") {
					var data = { data : dma_param_CLSFCTN.getJSON(), callbackFn: "scwin.insertSubGrid" };
					var options = { id : "SI0600P01",
									popupName : "구성원 목록",
									modal : true};
					com.openPopup("/ui/BMS/SI0600/SI0600P01.xml", options, data); 
				}	
			}
		}
	};
	
	
	scwin.insertSubGrid = function(obj) {
		var dLength = obj.length;
		for (i = 0; i < dLength; i++) {
			var data = dlt_EMER_MEMBER_INFO.getMatchedIndex("MEMER_ID", obj[i].MEMER_ID, true);
			if (data.length == 0) {
				obj[i].CLSFCTN_ID = dma_param_CLSFCTN.get("CLSFCTN_ID");
				obj[i].chk = "0";
				var idx = dlt_EMER_MEMBER_INFO.insertRow();
				dlt_EMER_MEMBER_INFO.setRowJSON(idx, obj[i], true);
			}
		}		
	}
	
	
	
	scwin.sub_SI0600T0K0_submitdone = function(e) {
		//트리는 getDataList 없어서 에러뜸
//		com.addGridForm(SI0600T0, SI0600F0, dma_SEQ_BMS_EMER_CLSFCTN_MST_0, "CLSFCTN_ID", "CLSFCTN_NM");
		var selectRowIdx = dlt_EMER_CLSFCTN_MST.getRowPosition();
		if (selectRowIdx === null) {
			com.alert("분류 목록 선택 후 추가하실 수 있습니다.")	
		} else {
			var currNm = SI0600T0.getSelectedNode().label;
			var currLevel = SI0600T0.getSelectedNode().depth;
			
			if (currLevel < 3) {
				scwin.selectLevel(currNm);
			} else {
				scwin.equalLevel();
			}
		}
	};
	
	scwin.sub_SI0600T0S0_submitdone = function(e) {
		com.executeSubmission(sub_SI0600T0R0);
	};
	
	
	scwin.sbx_ChoiceBox_onviewchange = function(info) {
		if (info.newValue == "ALL") {
