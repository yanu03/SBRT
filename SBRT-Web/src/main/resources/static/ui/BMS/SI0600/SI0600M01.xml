	}
	
	/**
	 * 메뉴레벨이 1 또는 2일 경우에 실행되는 함수
	 * Yes, No에 따라서 하위에 만들 것인지, 동일한 레벨로 만들 것인지 결정한다.
	 */
	scwin.selectLevel = function(returnValue) {
		com.confirm(returnValue + " 하위에 새로운 목록을 추가하시겠습니까? \n 아니오를 누르면 동일한 레벨에 목록이 생성됩니다.", "scwin.selectLevelCallbak");
	};
	
	scwin.selectLevelCallbak = function(value) {
		if (value == true) {
			scwin.lowerLevel();
		} else {
			scwin.equalLevel();
		}
	};	
	
	/**
	 * 하위 레벨의 노드에 메뉴를 추가한다.(포커스가 잡혔는데 depth가 1~2일 경우)
	 */
	scwin.lowerLevel = function() {
		
		var selectedLevel = SI0600T0.getSelectedNode().depth;
		
		var parent_value = SI0600T0.getSelectedNode().value;
		var seq = dma_SEQ_BMS_EMER_CLSFCTN_MST_0.get("SEQ");
		
		var selectedIndex = SI0600T0.getSelectedIndex();
		
		var selectedPath = dlt_EMER_CLSFCTN_MST.getCellData(selectedIndex-1, "path");
		
		var sort = SI0600T0.getSelectedNode().childNodes.length + 1;
		
		var tmpDataArr = dlt_EMER_CLSFCTN_MST.getAllJSON();
		
		var i = selectedIndex - 1;
		
		for (i = selectedIndex; i < tmpDataArr.length; i++) {
			if (selectedPath.length == tmpDataArr[i].path.length ){
				break;
			}

		}
		
		var idx = dlt_EMER_CLSFCTN_MST.insertRow(i);		
		
		dlt_EMER_CLSFCTN_MST.setCellData(idx, "LEVEL", selectedLevel+1);
		dlt_EMER_CLSFCTN_MST.setCellData(idx, "CLSFCTN_NM", "새 분류");
		dlt_EMER_CLSFCTN_MST.setCellData(idx, "PARENT_ID", parent_value);
		dlt_EMER_CLSFCTN_MST.setCellData(idx, "CLSFCTN_ID", seq);
		dlt_EMER_CLSFCTN_MST.setCellData(idx, "SORT", sort);

	};	
	
	scwin.equalLevel = function () {
		var selectedLevel = SI0600T0.getSelectedNode().depth;
		
		var parent_value = SI0600T0.getSelectedNode().parentNode.value;
		var seq = dma_SEQ_BMS_EMER_CLSFCTN_MST_0.get("SEQ");

		var selectedIndex = SI0600T0.getSelectedIndex();

		var selectedPath = dlt_EMER_CLSFCTN_MST.getCellData(selectedIndex-1, "path");
		
		// sort 수정 필요, 나머지 형제 노드들 sort값 +1 해줘야함
		var sort = parseInt(selectedPath.substring(selectedPath.length - 1 , selectedPath.length)) + 1;
		
		var tmpDataArr = dlt_EMER_CLSFCTN_MST.getAllJSON();
		
		var i = 0;
		var isSamed = false;
		
		
		for (i = 0; i < tmpDataArr.length; i++) {
			if(selectedPath == tmpDataArr[i].path){
				isSamed = true;
			}
			if(isSamed&&(selectedPath != tmpDataArr[i].path.substring(0,selectedPath.length))){
				break;	
			}
		}
		
		var idx = dlt_EMER_CLSFCTN_MST.insertRow(i);
		
		dlt_EMER_CLSFCTN_MST.setCellData(idx, "LEVEL", selectedLevel);
		dlt_EMER_CLSFCTN_MST.setCellData(idx, "CLSFCTN_NM", "새 분류");
		dlt_EMER_CLSFCTN_MST.setCellData(idx, "PARENT_ID", parent_value);
		dlt_EMER_CLSFCTN_MST.setCellData(idx, "CLSFCTN_ID", seq);
		dlt_EMER_CLSFCTN_MST.setCellData(idx, "SORT", sort);		
				
	};
	
