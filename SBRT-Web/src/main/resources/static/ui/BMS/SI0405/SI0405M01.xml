	scwin.sub_SI0405G0R0_submitdone = function(e) {
		com.setFocusedCell(SI0405G0, "REP_ROUT_ID");	

		var wayYn = dlt_BMS_REP_ROUT_MST.getCellData(SI0405G0.getFocusedRowIndex(), "WAY_YN");
