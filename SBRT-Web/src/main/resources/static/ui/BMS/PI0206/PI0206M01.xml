		<script type="text/javascript" lazy="false"><![CDATA[ 
	scwin.onpageload = function() {
		var autoOptions = {
			Main : { grid : PI0206G0, srchSbm : sub_PI0206G0R0, keyColumn : "REP_ROUT_ID", srchGrp : pgm_header, name :  "코스 목록"},
			Sub1 : { grid : PI0206G1, srchSbm : sub_PI0206G1R0, keyColumn : "VHC_ID",savSbm : sub_PI0206G1S0, keySbm : sub_PI0206G0K0, srchGrp : gr_sub_header, name :  "차량 정보"},
			Sub2 : { grid : PI0206G2, srchSbm : sub_PI0206G2R0, srchGrp : gr_sub_header, name :  "예약 상세정보"}
			
		 };				
		 
		var mainOptions = {
			RESERV : scwin.btn_resrv_onclick,
			RESERV_CNL : scwin.btn_resrvcnl_onclick
		};		 
		 
		com.setMainBtn2(wfm_mainBtn, gcm.DISP_TYPE.DUAL_GRID, autoOptions, mainOptions);		 
		
		var codeOptions = [ {code : "WAY_DIV", compID : "PI0206G0:WAY_DIV"},
		
							{code : "AREA", compID : "PI0206G1:AREA"},
							{code : "ROUT_DIV", compID : "PI0206G1:ROUT_DIV"},
							{code : "VHC_TYPE", compID : "PI0206G1:VHC_TYPE"},
							{code : "VHC_FUEL", compID : "PI0206G1:VHC_FUEL"},
							{code : "VHC_KIND", compID : "PI0206G1:VHC_KIND"},
							
							{code : "VHC_KIND", compID : "PI0206G2:VHC_KIND"},
							{code : "ROUT_DIV", compID : "PI0206G2:ROUT_DIV"},
							{code : "VHC_TYPE", compID : "PI0206G2:VHC_TYPE"},
							{code : "VHC_FUEL", compID : "PI0206G2:VHC_FUEL"},
							{code : "AREA", compID : "PI0206G2:AREA"},
							{code : "INST_LOC", compID : "PI0206G2:INST_LOC"},
							{code : "PROCE_RST", compID : "PI0206G2:PROCE_RST"}
		 ];	
		
		com.setCommonDtl(codeOptions, function() {
			com.searchGrid(PI0206G0, sub_PI0206G0R0);
		});			
		
		
		PI0206G0.mergeCol(0,false);
		PI0206G0.mergeCells("byrestriccol");
		PI0206G1.mergeCol(0,false);
		PI0206G1.mergeCol(1,false);
		PI0206G1.mergeCol(2,false);
		PI0206G1.mergeCells("byrestriccol");
		
		var subOptions = {
			SEARCH: {}
		};
		
		com.setSubBtn2(wfm_subBtn, autoOptions, subOptions);		
		com.executeSubmission(sub_PI0206G1R1);
		
		RSV_DT.setValue($p.getCurrentServerDate("yyyyMM"));
	};
	
	$p.executeSubmission(sub_PI0206SHI0);
	
	scwin.onpageunload = function() {
		
	};

	scwin.sub_PI0206G0R0_submitdone = function(e) {
		com.setFocusedCell(PI0206G0, "REP_ROUT_NM");
		com.executeSubmission(sub_PI0206G1R0);			
	};
	
	scwin.PI0206G0_onrowindexchange = function(row,oldRow) {
		//var focusOption = {keyMap : dma_subsearch};
		//com.changeDualGrid(PI0206G0, PI0206G1, sub_PI0206G1S0, sub_PI0206G1R0, focusOption, row, oldRow);		
	};
	
	scwin.sub_PI0206G1S0_submitdone = function(e) {
		com.saveDone(PI0206G1, sub_PI0206G1R0);	
	};
	
	scwin.btn_resrv_onclick = function() {
		var jsondata = PI0206G1.getCheckedJSON("chk");
		var data = {data : jsondata, callbackFn : "scwin.PI0206P0_callbackFn"};
		if(jsondata.length > 0){
			var options = { id : "PI0206P01",
							popupName : "예약 날짜",
							modal : true,
							width : 220, height: 350};
			com.openPopup("/cm/common/popUpReserve.xml", options, data);
		}else{
			com.alert('예약할 차량을 선택해 주세요.');
		}

	};
	
	scwin.PI0206P0_callbackFn = function(obj) {
		var rows = PI0206G1.getCheckedIndex("chk");
		var count = rows.length;
		if (count > 0 ) {
			for(i = 0; i<count; i++) {
				dlt_VHC_MST.setCellData(rows[i], "RSV_DT", obj.data[i].RSV_DT);
			}
		}
		com.saveGrid(PI0206G1, sub_PI0206G1S0);	
	}
	
	scwin.btn_resrvcnl_onclick = function() {
		var rows = PI0206G1.getCheckedIndex("chk");
		var count = rows.length;
		var str = count + " 건의 데이터를 예약 취소 하시겠습니까?";
		
		if(count > 0) {
			com.confirm(str, function(rtn) {
				if (rtn) {
					com.executeSubmission(sub_PI0206G1U0);
				}	
			});
		}
		
	}
	
	scwin.sub_PI0206G1U0_submitdone = function(e) {
		com.saveDone(PI0206G1, sub_PI0206G1R0);	
	};
	
	scwin.sbx_ChoiceBox_onviewchange = function(info) {
		if (info.newValue == "ALL") {
			autoChoiceBox.hide();
			search_input.setValue("");
			search_input.show("");
		} 
		else if (info.newValue == "COR_ID") {
			search_input.hide();
			autoChoiceBox.setNodeSet("data:dlt_searchitem", "COR_ID", "COR_ID");
			autoChoiceBox.show("");
		}
		else if (info.newValue == "COR_NM") {
			search_input.hide();
			autoChoiceBox.setNodeSet("data:dlt_searchitem", "COR_NM", "COR_NM");
			autoChoiceBox.show("");
		} 
	};
	
	scwin.PI0206G1_onrowindexchange = function(row,oldRow) {
		var focusOption = {keyMap : dma_subsearch2};
		com.changeDualGrid(PI0206G1, PI0206G2, null, sub_PI0206G2R0, focusOption, row, oldRow);			
	};
	
	scwin.PI0206G0_oncellclick = function(row,col,colId) {
      if (col != "0"){
         if(PI0206G0.getCellChecked(row, 'chk')==0){
            PI0206G0.setCellChecked(row, 'chk', true);
         }else if(PI0206G0.getCellChecked(row, 'chk')==1){
            PI0206G0.setCellChecked(row, 'chk', false);
         }
      }		
	};
	
	scwin.PI0206G1_oncellclick = function(row,col,colId) {
      if (col != "0"){
         if(PI0206G1.getCellChecked(row, 'chk')==0){
            PI0206G1.setCellChecked(row, 'chk', true);
         }else if(PI0206G1.getCellChecked(row, 'chk')==1){
            PI0206G1.setCellChecked(row, 'chk', false);
         }
      }		
	};
	
	scwin.RSV_STATE_onchange = function(info) {
		com.executeSubmission(sub_PI0206G2R0);
	};
	
	scwin.RSV_DT_onchange = function() {
		com.executeSubmission(sub_PI0206G2R0);		
	};
	
	scwin.sub_PI0206G1R0_submitdone = function(e) {
		com.setFocusedCell(PI0206G1, "VHC_ID");
	};
	]]></script>
