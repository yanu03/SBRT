	scwin.dateDisFormatter = function(data){
		if (data == "") return "";
		return data.substring(0,4) + "-" + data.substring(4,6) + "-" + data.substring(6,8) + " " + data.substring(8,10) + ":" + data.substring(10,12);
	}
	scwin.sendSTSDisFormatter = function(data){
		if (data == "SDS01"){
			return "예약전송대기중";
		}else if(data == "SDS02"){
			return "예약취소";
		}else if(data == "SDS03"){
			return "전송완료";
		}else{
			return "";
		}
	}
	scwin.sendDIVDisFormatter = function(data){
		if (data == "SDD01"){
			return "즉시전송";
		}else if(data == "SDD02"){
			return "예약전송";
		}else{
			return "";
		}
	}
	
