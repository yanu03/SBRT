	scwin.sbx_ChoiceBox_onviewchange = function(info) {
		
		if (info.newValue == "ALL") {
			autoChoiceBox.hide();
			search_input.setValue("");
			search_input.show("");
		} else if (info.newValue == "NOTICE_ID") {
			search_input.hide();
			autoChoiceBox.setNodeSet("data:dlt_searchitem", "NOTICE_ID", "NOTICE_ID");
