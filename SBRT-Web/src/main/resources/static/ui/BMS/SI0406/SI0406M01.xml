		<script type="text/javascript" lazy="false"><![CDATA[
	
/**
	
 * 표준링크, 노드가 없을 시 팝업에서 엑셀 업로드를 이용하여 가져옴
 * 지도에 표준 링크 데이터 뿌림
 * 지도 링크 선택하면 좌측하단 노선 링크정보에 하나씩 추가됨
 
 * @date 2022-01-12 신규생성
 * @author 양현우
 */
	scwin.onpageload = function() {
		
		var codeOptions = [{code : "NODE_TYPE", compID : "SI0406G1:NODE_TYPE"},{code : "WAY_DIV", compID : "SI0406G0:WAY_DIV"}];
		com.setCommonDtl(codeOptions, function() {
			com.searchGrid(SI0406G0, sub_SI0406G0R0);
		});	
		
		com.executeSubmission(sub_SI0406SHI0);
		
		var autoOptions = {
			Main : { grid : SI0406G0, srchSbm : sub_SI0406G0R0, srchGrp : pgm_header, keyColumn : "ROUT_ID", focusColumn : "ROUT_ID", name : "노선 목록"},
			Sub1 : { grid : SI0406G1, srchSbm : sub_SI0406G1R0, srchGrp : pgm_subList, savSbm : sub_SI0406G1S0, name :  "노선 링크정보"}
		 };
		 
		com.setMainBtn2(wfm_mainBtn, gcm.DISP_TYPE.DUAL_GRID2, autoOptions);

		var subOptions = {
			SEARCH: scwin.btn_sub_move, DEL:scwin.btn_sub_del
		};
		
		com.setSubBtn2(wfm_subBtn, autoOptions, subOptions);
		var subOptions2 = {
			SAVE: [{cbFnc:scwin.btn_sub2_link, nm:"표준링크", class:"share"}, {cbFnc:scwin.btn_sub2_node, nm:"표준노드", class:"share"}]
		};
		
		//com.setEnterKeyEvent(pgm_subList, scwin.btn_sub_search_onclick);
		
		com.setSubBtn2(wfm_subBtn2, autoOptions, subOptions2);
				
		SI0406G0.mergeCells("byrestriccol");
		
		routMap.initMap("map_SI0406", {width:"100%"
			, height:"100%"
			, onClick: scwin.onClickMap
		});
		

		scwin.disp_check_all();
		
		com.executeSubmission(sub_SI0406G2R0);
	};

	
	scwin.onpageunload = function() {
		
	};
	

	scwin.sub_SI0406G0R0_submitdone = function(e) {
		com.setFocusedCell(SI0406G0, "ROUT_ID");	
	};
	
	scwin.sbx_ChoiceBox_onviewchange = function(info) {
		if (info.newValue == "ALL") {
			autoChoiceBox.hide();
			search_input.setValue("");
			search_input.show("");
		} 
		else if (info.newValue == "ROUT_ID") {
			search_input.hide();
			autoChoiceBox.setNodeSet("data:dlt_routItem", "ROUT_ID", "ROUT_ID");
			autoChoiceBox.show("");
		}
		else if (info.newValue == "ROUT_NM") {
			search_input.hide();
			autoChoiceBox.setNodeSet("data:dlt_routItem", "ROUT_NM", "ROUT_NM");
			autoChoiceBox.show("");
		} else if (info.newValue == "REP_ROUT_NM") {
			search_input.hide();
			autoChoiceBox.setNodeSet("data:dlt_routItem", "REP_ROUT_NM", "REP_ROUT_NM");
			autoChoiceBox.show("");
		}		
	};
	

	scwin.dma_disp_check_onviewchange = function(info) {
		routMap.setDispCheck("map_SI0406", disp_check.getValue());
		routMap.drawRoute("map_SI0406", SI0406G1, -1, true);
	}
	
	scwin.SI0406G0_onrowindexchange = function(row,oldRow) {
		routMap.initMapInfo("map_SI0406");
		dma_sub_search.set("ROUT_ID", dlt_BMS_ROUT_MST.getCellData(row,"ROUT_ID"));
		dma_sub_search.set("WAY_DIV", dlt_BMS_ROUT_MST.getCellData(row,"WAY_DIV"));
		var focusOption = {keyMap : dma_sub_search};
		com.changeDualGrid(SI0406G0, SI0406G1, sub_SI0406G1S0, sub_SI0406G1R0, focusOption, row, oldRow);
	};
	
	scwin.btn_sub_search_onclick = function(){
		
		com.setFocusedCell2(SI0406G1, "NODE_NM", dma_sub_search.get("CONTENT"));
	}
	
	scwin.SI0406G1_onrowindexchange = function(row,oldRow) {
	
		routMap.focusNode("map_SI0406",SI0406G1,row);
	};
	
	scwin.sub_SI0406G1R0_submitdone = function(e) {
		//routMap.drawRoute("map_SI0406", SI0406G1, -1, false);
		if(dlt_BMS_ROUT_NODE_CMPSTN.getTotalRow() == 0){
			routMap.initDisplay("map_SI0406");
		}
		else {
			//com.setFocusedCell(SI0406G1, "NODE_ID");
			if(SI0406G1.getFocusedRowIndex() == -1 ){
				SI0406G1.setFocusedCell( 0, "NODE_ID");
			}
			else {
				SI0406G1.setFocusedCell( SI0406G1.getFocusedRowIndex(), "NODE_ID");
			}
		}
		scwin.btn_sub_move();
	};
	
	scwin.sub_SI0406G1S0_submitdone = function(e) {
		var sCode = com.getResultCode(e);
		if (sCode == gcm.MESSAGE_CODE.STATUS_SAVE) {
			routMap.initMapInfo("map_SI0406");
			com.saveDone(SI0406G1,sub_SI0406G1R0);
		}
	}
			
	scwin.btn_integration_onclick = function() {
		if(dlt_BMS_ROUT_NODE_CMPSTN.getTotalRow()>0){
			var str = "갱신 후에는 되돌릴수 없습니다. 정말 갱신하시겠습니까?";
			com.confirm(str, function(rtn) {
				if (rtn) {
					var val_NODE_ID = dlt_BMS_ROUT_NODE_CMPSTN.getColData("NODE_ID");
					//dma_sub_search.set("NODE_IDS", val_NODE_ID);
					dma_sub_search.set("ROUT_ID", dlt_BMS_ROUT_MST.getCellData(SI0406G0.getFocusedRowIndex(), "ROUT_ID"));
					dma_sub_search.set("INT_ROUT_ID", dlt_BMS_ROUT_MST.getCellData(SI0406G0.getFocusedRowIndex(), "INT_ROUT_ID"));
					dma_sub_search.set("AREA", dlt_BMS_ROUT_MST.getCellData(SI0406G0.getFocusedRowIndex(), "AREA"));
					var param = dma_sub_search.get("ROUT_ID");
					if (param != "") {
						var data = { data : dma_sub_search.getJSON(), callbackFn : "scwin.insertNode" };
						var options = { id : "SI0406P01",
										popupName : "노드연계", 
										modal : true};
						com.openPopup("/ui/BMS/SI0406/SI0406P01.xml", options, data);
					}
				}
			});
		}
		else {
			var val_NODE_ID = dlt_BMS_ROUT_NODE_CMPSTN.getColData("NODE_ID");
			//dma_sub_search.set("NODE_IDS", val_NODE_ID);
			dma_sub_search.set("INT_ROUT_ID", dlt_BMS_ROUT_MST.getCellData(SI0406G0.getFocusedRowIndex(), "INT_ROUT_ID"));
					
			var param = dma_sub_search.get("ROUT_ID");
			if (param != "") {
				var data = { data : dma_sub_search.getJSON(), callbackFn : "scwin.insertNode" };
				var options = { id : "SI0406P01",
								popupName : "노드연계", 
								modal : true};
				com.openPopup("/ui/BMS/SI0406/SI0406P01.xml", options, data);
			}
		}
	}
	
	scwin.btn_del_onclick = function() {

	};
	
	scwin.btn_sub_del = function() {
		com.delGrid(SI0406G1,"",
			function() { 
				var index = SI0406G1.getFocusedRowIndex();
				routMap.focusNode("map_SI0406",SI0406G1,index,false); 
			}
		);
	};
	
	
	scwin.btn_cancel_onclick = function() {
		var idx = SI0406G1.getModifiedIndex().length;
		
		if (idx > 0) {
			var str =com.strModifiedCnt(SI0406G1) + "의 저장하지 않은 데이터가 있습니다. 취소 하시겠습니까?";
			com.confirm(str, function(rtn) {
				if (rtn) {
					com.clearGrid(SI0406G1);
					com.searchGrid(SI0406G0, sub_SI0406G0R0);22
				}
			});
		}

	};
	
	scwin.btn_sub_move = function() {
		if(com.isEmpty(dma_sub_search.get("CONTENT"))==false){
			com.gridMove(SI0406G1, "NODE_ID,NODE_NM", dma_sub_search.get("CONTENT"), SI0406G1.getFocusedRowIndex());
		}
	};
	
	//맵 클릭 이벤트
	scwin.onClickMap = function(e) {
	
	}
	
	scwin.insertNode = function(obj){
		dlt_BMS_ROUT_NODE_CMPSTN.removeAll();
		var route_id = dlt_BMS_ROUT_MST.getCellData(SI0406G0.getFocusedRowIndex(), "ROUT_ID");
		var area = dlt_BMS_ROUT_MST.getCellData(SI0406G0.getFocusedRowIndex(), "AREA");
		routMap.insertNodeAll("map_SI0406",SI0406G1,obj,route_id,area);
	}
	


	scwin.addPathClick = function(){

	}
	
	scwin.subGridUp = function(){
		var idx = SI0406G1.getFocusedRowIndex();
		com.getGridViewDataList(SI0406G1).rowMoveUp(idx);
	}
	
	scwin.subGridDown = function(){
		var idx = SI0406G1.getFocusedRowIndex();
		com.getGridViewDataList(SI0406G1).rowMoveDown(idx);
	}
	
	scwin.disp_check_all = function(){
		disp_check.getCheckboxList()[0].checked = true;
		disp_check.getCheckboxList()[1].checked = true;
		disp_check.getCheckboxList()[2].checked = true;
		routMap.setDispCheck("map_SI0406", disp_check.getValue());
	}
	
	scwin.btn_sub2_link = function() {
		
		var data = { callbackFn : "scwin.insertMockLink" };
		var options = { id : "SI0406P01",
						popupName : "표준링크 목록",
						width : 600,
						height : 500,
						modal : true };
		com.openPopup("/ui/BMS/SI0406/SI0406P01.xml", options, data);	
	}	
	
	scwin.btn_sub2_node = function() {

		
	}

]]></script>
