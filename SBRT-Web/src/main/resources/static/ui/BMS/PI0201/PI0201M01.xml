	scwin.onpageload = function() {
		com.executeSubmission(sub_PI0201G0R0);
		
		var autoOptions = {
			Main : { grid : PI0201G0, frm : PI0201F0, srchSbm : sub_PI0201G0R0, savSbm : sub_PI0201G0S0, keySbm : sub_PI0201G0K0 
