		 };
		 
		com.setMainBtn2(wfm_mainBtn, gcm.DISP_TYPE.SINGLE_GRID_FORM, autoOptions);
				
		routMap.initMap2("map_VH0400", 
				{width:"100%"
				, height:"100%"
				, onClick: scwin.onClickMap
		});
		
		var codeOptions = [ {code : "OPER_EVT_TYPE", compID : "VH0400G0:EVT_TYPE" }
		 ];			
		 
		com.setCommonDtl(codeOptions, function() {
			com.searchGrid(VH0400G0, sub_VH0400G0R0);
		});

	};	
	
	scwin.VH0400G0_onrowindexchange = function(row,oldRow) {
		dma_search.set("CONTENT1", dlt_BRT_OPER_EVENT_HIS.getCellData(row, "REP_ROUT_ID"));
		dma_search.set("CONTENT2", dlt_BRT_OPER_EVENT_HIS.getCellData(row, "ROUT_ID"));
		dma_search.set("CONTENT3", dlt_BRT_OPER_EVENT_HIS.getCellData(row, "NODE_TYPE"));
		$p.executeSubmission(sub_selectNodeListByRepRout);			
	};
	
	scwin.sub_VH0400G0R0_submitdone = function(e) {
		com.setFocusedCell(VH0400G0, "OPER_DT");
		
		var eventType = "";
		var eventMessage = "";
		for (var i=0; i<dlt_BRT_OPER_EVENT_HIS.getDataLength(); i++) {
			eventType = dlt_BRT_OPER_EVENT_HIS.getCellData(i, "EVT_TYPE");
			
			switch(eventType) {
				case "ET001":
				eventMessage = dlt_BRT_OPER_EVENT_HIS.getCellData(i, "NODE_NM") + "에 도착하였습니다.";
				break;
				
				case "ET002":
				eventMessage = dlt_BRT_OPER_EVENT_HIS.getCellData(i, "NODE_NM") + "에서 출발하였습니다.";
				break;
				
				case "ET003":
				eventMessage = "기점에 도착하였습니다.";
				break;
				
				case "ET004":
				eventMessage = "기점에서 출발하였습니다.";
				break;
				
				case "ET005":
				eventMessage = "종점에 도착하였습니다.";
				break;
				
				case "ET006":
				eventMessage = "종점에서 출발하였습니다.";
				break;
				
				case "ET007":
				eventMessage = "현재속도 " + dlt_BRT_OPER_EVENT_HIS.getCellData(i, "CUR_SPD")+ "km/h, 평균 "+ 
								dlt_BRT_OPER_EVENT_HIS.getCellData(i, "EVT_DATA") + "km/h 입니다.";
				break;
				
				case "ET008":
				//eventMessage = dlt_BRT_OPER_EVENT_HIS.getCellData(i, "ORGA_NM") + " 음성 방송 출력되었습니다.";
				eventMessage = "음성 방송 출력되었습니다. " + "("+dlt_BRT_OPER_EVENT_HIS.getCellData(i, "EVT_DATA") +")";
				break;
				
				case "ET009":
				eventMessage = "버스 문이 열렸습니다.";
				break;
				
				case "ET010":
				eventMessage = "버스 문이 닫혔습니다.";
				break;
			}//end switch
			
			dlt_BRT_OPER_EVENT_HIS.setCellData(i, "EVT_DATA", eventMessage);
		}//end for		
		
	};
	
	scwin.sub_selectNodeListByRepRout_submitdone = function(e) {
		routMap.showRoute("map_VH0400", dlt_BMS_ROUT_NODE_CMPSTN.getAllJSON(), null, "ONLYLINE");
		
		var focusIdx = VH0400G0.getFocusedRowIndex();
		var focusDataList = dlt_BRT_OPER_EVENT_HIS.getRowJSON(focusIdx);
		
		routMap.showVehicle3("map_VH0400", focusDataList, VH0400G0);
	};
	
	]]></script>
