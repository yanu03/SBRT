		<script type="text/javascript" lazy="false"><![CDATA[
	var circle = null;
	scwin.onpageload = function() {
		 var autoOptions = {
			Main : { grid : MO0600G0, srchSbm : sub_MO0600G0R0, keyColumn : "VHC_ID", srchGrp : pgm_header, name : "차량 목록"}
		 };
		 
