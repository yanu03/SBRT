					<w2:columnInfo>
						<w2:column id="FCLT_ID" name="" dataType="text"></w2:column>
						<w2:column id="INTG_FCLT_ID" name="" dataType="text"></w2:column>
						<w2:column id="PK002" name="" dataType="text"></w2:column>
						<w2:column id="PK022" name="" dataType="text"></w2:column>
						<w2:column id="PK047" name="" dataType="text"></w2:column>
					</w2:columnInfo>
				</w2:dataList>
				<w2:dataMap baseNode="map" id="dma_search">
					<w2:keyInfo>
						<w2:key id="TYPE" name="name1" dataType="text"></w2:key>
						<w2:key id="CONTENT" name="name2" dataType="text"></w2:key>
						<w2:key id="USE_YN" name="name3" dataType="text"></w2:key>
						<w2:key id="FCLT_ID" name="name3" dataType="text"></w2:key>
						<w2:key id="PK002" name="name3" dataType="text"></w2:key>
						<w2:key id="PK022" name="name3" dataType="text"></w2:key>
						<w2:key id="PK047" name="name3" dataType="text"></w2:key>
					</w2:keyInfo>
				</w2:dataMap>
			</w2:dataCollection>
			<w2:workflowCollection></w2:workflowCollection>
			<xf:submission id="sub_selectIntgInfo" ref='data:json,dma_search' target="data:json,dlt_airconControl" action="/mo/MO0205P0R0" method="post"
				mediatype="application/json" encoding="UTF-8" instance="" replace="" errorHandler="" customHandler="" mode="asynchronous" processMsg="조회중입니다..."
				ev:submit="" ev:submitdone="scwin.sub_selectIntgInfo_submitdone" ev:submiterror="" abortTrigger="">
			</xf:submission>
			<xf:submission id="sub_saveIntgInfo" ref='data:json,dlt_airconControl' target="data:json,dma_result" action="/mo/airconControl" method="post"
				mediatype="application/json" encoding="UTF-8" instance="" replace="" errorHandler="" customHandler="" mode="asynchronous" processMsg="전송중입니다..."
				ev:submit="" ev:submitdone="scwin.sub_saveIntgInfo_submitdone" ev:submiterror="" abortTrigger="">
			</xf:submission>
		</xf:model>
		<script type="text/javascript" lazy="false"><![CDATA[
        
	scwin.onpageload = function() {
		scwin.param = com.getParameter();
		
		scwin.closeCallbackFncName = scwin.param.callbackFn;
		com.setEnterKeyEvent(popup_header, scwin.btn_search_onclick);
		dma_search.set("FCLT_ID", scwin.param.fcltId);
		
		
		dma_search.set("PK002", scwin.param.power);
		dma_search.set("PK022", scwin.param.curDegree);
		dma_search.set("PK047", scwin.param.setDegree);
		
		com.executeSubmission(sub_selectIntgInfo);
	};
	
	scwin.onpageunload = function() {
		
	};
	
	scwin.sub_selectIntgInfo_submitdone = function(e) {
		scwin.param = com.getParameter();
		dlt_airconControl.setCellData(0, "FCLT_ID", scwin.param.fcltId);
		dlt_airconControl.setCellData(0, "PK002", scwin.param.power);
		dlt_airconControl.setCellData(0, "PK022", scwin.param.curDegree);
		dlt_airconControl.setCellData(0, "PK047", scwin.param.setDegree);
		dlt_airconControl.modifyAllStatus("R");
	};
	
	//확인 버튼 onclick 이벤트
	scwin.btn_confirm_onclick = function() {
		com.executeSubmission(sub_saveIntgInfo);
	};
	
	//취소 버튼 onclick 이벤트
	scwin.btn_cancel_onclick = function() {
		com.closePopup();
	};
	
	scwin.sub_saveIntgInfo_submitdone = function(e) {
		com.closePopup();
	};
	
	scwin.T0_PK002_onviewchange = function(info) {
		dlt_airconControl.setCellData(0, "PK002", info.newValue);
	};
	
	scwin.T0_PK047_onviewchange = function(info) {
		dlt_airconControl.setCellData(0, "PK047", info.newValue);
	};
	
	scwin.T0_PK022_onviewchange = function(info) {
		dlt_airconControl.setCellData(0, "PK022", info.newValue);
	};
	]]></script>
	</head>
