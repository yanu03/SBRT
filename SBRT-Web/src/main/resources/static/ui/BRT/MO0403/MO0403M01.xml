					width : 600, height: 500 };
		com.openPopup("/ui/BMS/MO0403/MO0403P01.xml", opt, data);
	};
	
	//찾기 버튼 콜백함수
	scwin.searchCompCallback = function(returnValue) {
		COMP_NM.setValue(returnValue[0].COMP_NM);
		dlt_BMS_VHC_MST.setCellData(MO0403G0.getFocusedRowIndex(), "COMP_ID", returnValue[0].COMP_ID);
		dlt_BMS_VHC_MST.setCellData(MO0403G0.getFocusedRowIndex(), "AREA", returnValue[0].AREA);
	};

	scwin.COMP_NM_onfocus = function(e) {
		scwin.btn_searchComp_onclick();
	};
	
	scwin.sbx_ChoiceBox_onviewchange = function(info) {		
		if (info.newValue == "ALL") {
			acb_autoChoiceBox.hide();
			search_input.setValue("");
			search_input.show("");
		} else if (info.newValue == "VHC_ID") {
			search_input.hide();
			acb_autoChoiceBox.setNodeSet("data:dlt_vhcSearchItem", "VHC_ID", "VHC_ID");
			acb_autoChoiceBox.show("");
		} else if (info.newValue == "VHC_NO") {
			search_input.hide();
			acb_autoChoiceBox.setNodeSet("data:dlt_vhcSearchItem", "VHC_NO", "VHC_NO");
			acb_autoChoiceBox.show("");
		}
	};
	
	scwin.MO0403G0_onrowindexchange = function(row, oldRow) {
		dma_search.set("VHC_ID", dlt_BMS_VHC_MST.getCellData(row, "VHC_ID"));
		com.changeSingleGrid(MO0403G0, row, oldRow);
	};
	
	scwin.btn_sendMsg_onclick = function() {
		var sendMessage = "";
		
		if (messageText.getDisabled() == true) {
			sendMessage = messageSelect.getValue();
		}
		//직접 입력
		else if (messageText.getDisabled() == false) {
			sendMessage = messageText.getValue();
		}
		dma_search.set("MSG_CONTS", sendMessage);
		
		com.executeSubmission(sub_MO0403SCK);
	}
	
	scwin.messageSelect_onchange = function(info) {
		var message = messageSelect.getValue();
		
		if (message != "") {
			messageText.setDisabled(true);	
		} else if (message == "") {
			messageText.setDisabled(false);
		}
		
	};
