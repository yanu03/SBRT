						<w2:column id="NODE_SN" name="노드순번" dataType="number" defaultValue="0"></w2:column>
						<w2:column id="NODE_NM" name="노드명" dataType="text"></w2:column>
						<w2:column id="NODE_TYPE" name="노드유형" dataType="text"></w2:column>
						<w2:column id="LINK_ID" name="링크ID" dataType="text"></w2:column>
						<w2:column id="STTN_ID" name="정류소ID" dataType="text"></w2:column>
						<w2:column id="STTN_NO" name="정류소번호" dataType="text"></w2:column>
						<w2:column id="CRS_ID" name="교차로ID" dataType="text"></w2:column>
						<w2:column id="GPS_X" name="경도" dataType="text"></w2:column>
						<w2:column id="GPS_Y" name="위도" dataType="text"></w2:column>
						<w2:column id="ROUT_LEN" name="노선거리" dataType="number" defaultValue="0"></w2:column>
						<w2:column id="AREA" name="권역" dataType="text"></w2:column>
						<w2:column id="MORN_STD" name="집중모니터링기준" dataType="text"></w2:column>
					</w2:columnInfo>
				</w2:dataList>
				<w2:dataMap baseNode="map" id="dma_sub_search">
					<w2:keyInfo>
						<w2:key id="CONTENT" name="name2" dataType="text"></w2:key>
						<w2:key id="COR_ID" name="코스ID" dataType="text"></w2:key>
						<w2:key id="ROUT_IDS" name="노선IDS" dataType="text"></w2:key>
					</w2:keyInfo>
				</w2:dataMap>
			</w2:dataCollection>
			<w2:workflowCollection></w2:workflowCollection>
			<xf:submission id="sub_AL0103G0R0" ref="data:json,dma_search" target="data:json,dlt_BRT_COR_MST" action="/al/AL0103G0R0"
				method="post" mediatype="application/json" encoding="UTF-8" instance="" replace="" errorHandler="" customHandler="" mode="asynchronous"
				processMsg="조회 중입니다..." ev:submit="" ev:submitdone="scwin.sub_AL0103G0R0_submitdone" ev:submiterror="" abortTrigger="">
			</xf:submission>
			<xf:submission id="sub_AL0103G0S0" ref='data:json,{"action":"modified","id":"dlt_BRT_COR_MST"}' target="data:json,dma_result"
				action="/al/AL0103G0S0" method="post" mediatype="application/json" encoding="UTF-8" instance="" replace="" errorHandler=""
				customHandler="" mode="asynchronous" processMsg="저장중입니다..." ev:submit="" ev:submitdone="scwin.sub_AL0103G0S0_submitdone" ev:submiterror=""
				abortTrigger="">
			</xf:submission>
			<xf:submission id="sub_AL0103G0K0" ref="" target="data:json,dma_SEQ_BRT_COR_MST_0" action="/al/AL0103G0K0" method="post"
				mediatype="application/json" encoding="UTF-8" instance="" replace="" errorHandler="" customHandler="" mode="asynchronous" processMsg=""
				ev:submit="" ev:submitdone="scwin.sub_AL0103G0K0_submitdone" ev:submiterror="" abortTrigger="">
			</xf:submission>
			<xf:submission id="sub_AL0103SHI0" ref="" target="data:json,dlt_searchitem" action="/al/AL0103SHI0" method="post"
				mediatype="application/json" encoding="UTF-8" instance="" replace="" errorHandler="" customHandler="" mode="asynchronous" processMsg=""
				ev:submit="" ev:submitdone="" ev:submiterror="" abortTrigger="">
			</xf:submission>
			<xf:submission id="sub_AL0103G1R0" ref="data:json,dma_sub_search" target="data:json,dlt_BRT_COR_DTL_INFO"
				action="/al/AL0103G1R0" method="post" mediatype="application/json" encoding="UTF-8" instance="" replace="" errorHandler=""
				customHandler="" mode="asynchronous" processMsg="" ev:submit="" ev:submitdone="scwin.sub_AL0103G1R0_submitdone" ev:submiterror=""
				abortTrigger="">
			</xf:submission>
			<xf:submission id="sub_AL0103G2R0" ref="data:json,dma_sub_search" target="data:json,dlt_BMS_ROUT_NODE_DISP_VW"
				action="/rout/selectNodeDispListByRouts" method="post" mediatype="application/json" encoding="UTF-8" instance="" replace="" errorHandler=""
				customHandler="" mode="asynchronous" processMsg="조회 중입니다.." ev:submit="" ev:submitdone="scwin.sub_AL0103G2R0_submitdone"
				ev:submiterror="" abortTrigger="">
			</xf:submission>
			<xf:submission id="sub_AL0103G1S0" ref="data:json,dlt_BRT_COR_DTL_INFO" target="data:json,dma_result" action="/al/AL0103G1S0" method="post" mediatype="application/json" encoding="UTF-8"
				instance="" replace="" errorHandler="" customHandler="" mode="asynchronous" processMsg="저장중입니다..." ev:submit="" ev:submitdone="scwin.sub_AL0103G1S0_submitdone"
				ev:submiterror="" abortTrigger="">
			</xf:submission>
		</xf:model>
		<script type="text/javascript" lazy="false"><![CDATA[
        
    var AL0103_isMapInit = false;
	scwin.onpageload = function() {
		var autoOptions = {
			Main : { grid : AL0103G0, frm : AL0103F0, srchSbm : sub_AL0103G0R0, savSbm : sub_AL0103G0S0, 
					 keySbm : sub_AL0103G0K0, srchGrp : pgm_header , keyColumn : "COR_ID", focusColumn : "COR_NM", name : "코스 관리"},
			Sub1 : { grid : AL0103G1, srchSbm : sub_AL0103G1R0, srchGrp : pgm_subList, savSbm : sub_AL0103G1S0,  name : "관련 노선" }	
		 };
		 
		var mainOptions = {
			SEARCH : {}
		};
		
		var codeOptions = [ 
							{code : "ROUT_TYPE", compID : "AL0103G1:ROUT_TYPE" },
							{code : "COR_TYPE", compID : "COR_TYPE, AL0103G0:COR_TYPE" },
							{code : "WAY_DIV", compID : "AL0103G1:WAY_DIV" },						
							{code : "JIT_DSPCH_YN", compID : "AL0103G1:JIT_DSPCH_YN" }						
													
						];	
							
		com.setMainBtn2(wfm_mainBtn, gcm.DISP_TYPE.DUAL_GRID_FORM, autoOptions, mainOptions, codeOptions);
	
		var mainSrhOptions = {
		};		
		com.setMainSrhBtn(wfm_mainSearchBtn, gcm.DISP_TYPE.DUAL_GRID_FORM, autoOptions, mainSrhOptions, codeOptions);
		com.setCommonDtl(codeOptions, function() {
			com.searchGrid(AL0103G0, sub_AL0103G0R0);
		});	
		
		com.executeSubmission(sub_AL0103SHI0);
		
		var subOptions = {
			SEARCH: scwin.btn_sub_move,
			ADD: scwin.btn_add2_onclick, 
			DEL:{},
        	SAVE:[{cbFnc:scwin.btn_up_onclick, nm:"상",class:"up"},{cbFnc:scwin.btn_down_onclick,nm:"하",class:"down"}
               ]			
		}

      	com.setSubBtn2(wfm_subBtn, autoOptions, subOptions);	
				
		//$("#AL0103_TAB").attr("style", "width:200px; height:27px;  float:left");
		pgm_sub_subList2.hide();
		
		//탭 목록 클릭 이벤트
		$("#AL0103_TAB").find("li").click( function() {
			var li_id = this.id;
			$("#AL0103_TAB").find("li").removeClass("on");
			$("#AL0103_TAB").find("#"+li_id).addClass("on");
			
			if(li_id == "li_cos") {
				pgm_sub_subList.show("");
				pgm_sub_subList2.hide();
				gr_sub_header.show("");
			}
			else if(li_id == "li_map") {
				pgm_sub_subList.hide();

				var val_ROUT_ID = dlt_BRT_COR_DTL_INFO.getColData("ROUT_ID");
				dma_sub_search.set("ROUT_IDS", val_ROUT_ID);
		//		dma_sub_search.set("TYPE", "DISP_NODE");
				com.executeSubmission(sub_AL0103G2R0);
				setTimeout(function() {
					if(AL0103_isMapInit==false){
						routMap.initMap("map_AL0103", {width:"100%"
							, height:"100%"
							, onClick: scwin.onClickMap
							, nodeChangeCb: scwin.nodeChangeCb
							, draggable: false
						});
						AL0103_isMapInit = true;
					}
					routMap.showRoute("map_AL0103", dlt_BMS_ROUT_NODE_DISP_VW.getAllJSON(), dma_sub_search.get("STTN_ID"));
				},100);
				pgm_sub_subList2.show("");
				gr_sub_header.hide();
				//com.setFocusedCell(al0103G1, "UPD_DTM");
			}
		});
	};
	
	scwin.onpageunload = function() {
		
	};
	
	scwin.sub_AL0103G0R0_submitdone = function(e) {
		com.setFocusedCell(AL0103G0, "COR_ID");
		for(var i = 0; i < dlt_BRT_COR_MST.getRowCount(); i++) {
			var corColor = dlt_BRT_COR_MST.getCellData(i, "COR_COLOR");
			AL0103G0.setCellColor(i, "COR_COLOR", corColor);
		}
	};
	
	scwin.sub_AL0103G0S0_submitdone = function(e) {
		
		var sCode = com.getResultCode(e);
		if (sCode == gcm.MESSAGE_CODE.STATUS_SAVE) {
			com.saveDone(AL0103G0,sub_AL0103G0R0);
		}
	};
	
	scwin.sub_AL0103G0K0_submitdone = function(e) {
		com.addGridForm(AL0103G0, AL0103F0, dma_SEQ_BRT_COR_MST_0, "COR_ID", "COR_NM");
	};
	
	scwin.sbx_ChoiceBox_onviewchange = function(info) {
		if (info.newValue == "ALL") {
			autoChoiceBox.hide();
			search_input.setValue("");
			search_input.show("");
		} 
		else if (info.newValue == "COR_ID") {
			search_input.hide();
			autoChoiceBox.setNodeSet("data:dlt_searchitem", "COR_ID", "COR_ID");
			autoChoiceBox.show("");
		} else if (info.newValue == "COR_NM") {
			search_input.hide();
			autoChoiceBox.setNodeSet("data:dlt_searchitem", "COR_NM", "COR_NM");
			autoChoiceBox.show("");
		}
	};
	
	scwin.AL0103G0_onrowindexchange = function(row,oldRow) {
		var focusOption = {keyMap : dma_sub_search};
		com.changeDualGrid(AL0103G0, AL0103G1, null, sub_AL0103G1R0, focusOption, row, oldRow);
		
		var corType = dlt_BRT_COR_MST.getCellData(row, "COR_TYPE");
		
		//코스유형이 노선이 아닐 경우 소요시간 disable처리
		if(corType == "CT002" || corType == "CT003" || corType == "CT004" || corType == null || corType == ""){
			MOVE_TM.setDisabled(true);
		}else if(corType == "CT001"){
			MOVE_TM.setDisabled(false);
		}
	};
	
	scwin.sub_AL0103G1R0_submitdone = function(e) {
		$("#AL0103_TAB").find("#li_map").removeClass("on");
		$("#AL0103_TAB").find("#li_cos").addClass("on");	
		pgm_sub_subList.show("");
		pgm_sub_subList2.hide();
		gr_sub_header.show("");
		scwin.btn_sub_move();
	};
	
	scwin.sub_AL0103G2R0_submitdone = function(e) {
		routMap.showRoute("map_AL0103", dlt_BMS_ROUT_NODE_DISP_VW.getAllJSON(), dma_sub_search.get("STTN_ID"));
	};
	
	scwin.btn_sub_move = function() {
		if(com.isEmpty(dma_sub_search.get("CONTENT"))==false){
			com.gridMove(AL0103G1, "ROUT_ID,ROUT_NM", dma_sub_search.get("CONTENT"), AL0103G1.getFocusedRowIndex());
		}
	}
	
	scwin.btn_add2_onclick = function() {
		var focusRowIdx = scwin.idxRow;
		if (focusRowIdx == -1) {
			com.alert("코스 목록 선택후 관련 노선을 등록하실 수 있습니다.");
		} else {
			var rowStatus = AL0103G0.getRowStatus(focusRowIdx);
			if (rowStatus == "C") {
				com.alert("코스 등록 후에 사용 가능합니다.");
			} else {
				var val_ROUT_ID = dlt_BRT_COR_DTL_INFO.getColData("ROUT_ID");
				var repRoutNm = dlt_BRT_COR_MST.getCellData(AL0103G0.getFocusedRowIndex(), "REP_ROUT_NM");
				if (true) {
					var data = { data : val_ROUT_ID, data2 : repRoutNm, callbackFn : "scwin.insertGRG" };
					var options = { id : "popUpRoutLists",
									popupName : "노선 목록", 
									modal : true };
					com.openPopup("/cm/common/popupRoutLists.xml", options, data);
				}
			}
		}		
	};	
	
	//서브 그리드 추가 콜백함수
	//팝업창의 data를 서브 그리드에 추가
	scwin.insertGRG = function(obj) {
		var dLength = obj.length;
		for (i = 0; i < dLength; i++) {
			var data = dlt_BRT_COR_DTL_INFO.getMatchedIndex("ROUT_ID", obj[i].ROUT_ID, true);
			if (data.length == 0) {
				var idx = dlt_BRT_COR_DTL_INFO.insertRow();
				dlt_BRT_COR_DTL_INFO.setCellData(idx, "COR_ID", dma_sub_search.get("COR_ID"));
				dlt_BRT_COR_DTL_INFO.setCellData(idx, "ROUT_SN", idx+1);
				dlt_BRT_COR_DTL_INFO.setRowJSON(idx, obj[i], true);
			}
		}
	};	
		
	scwin.btn_up_onclick = function() {
	    var idx = AL0103G1.getFocusedRowIndex();
	    com.getGridViewDataList(AL0103G1).rowMoveUp(idx);
	    var data = dlt_BRT_COR_DTL_INFO.getAllJSON();
	    var count = data.length;
	    
	    for(i=0; i<count; i++) {
		    dlt_BRT_COR_DTL_INFO.setCellData(i, "ROUT_SN", i+1);	
		   } 
	   
	}
	
	scwin.btn_down_onclick = function () {
  	    var idx = AL0103G1.getFocusedRowIndex();
   		com.getGridViewDataList(AL0103G1).rowMoveDown(idx);	
	    var data = dlt_BRT_COR_DTL_INFO.getAllJSON();
	    var count = data.length;
	    
	    for(i=0; i<count; i++) {
		    dlt_BRT_COR_DTL_INFO.setCellData(i, "ROUT_SN", i+1);	
		   }    
	}
	
	
	scwin.REP_ROUT_NM_SEARCH_onclick = function(e) {
		var data = {callbackFn : "scwin.REP_ROUT_NM_SEARCH_callback" };
		var options = { id : "popUpRepRoutList",
						popupName : "대표노선 목록", 
						modal : true};
		com.openPopup("/cm/common/popUpRepRoutList.xml", options, data);				
	};	
	
	scwin.REP_ROUT_NM_SEARCH_callback = function(obj) {
		dlt_BRT_COR_MST.setCellData(AL0103G0.getFocusedRowIndex(), "REP_ROUT_ID", obj[0].REP_ROUT_ID);
		REP_ROUT_NM.setValue(obj[0].REP_ROUT_NM);
	};
	
	scwin.REP_ROUT_NM_onfocus = function(e) {
		scwin.REP_ROUT_NM_SEARCH_onclick();
	};				
	
	scwin.sub_AL0103G1S0_submitdone = function(e) {
		com.saveDone(AL0103G1, sub_AL0103G1R0);		
	};
	
	//코스유형이 노선이 아닐 경우 소요시간 disable처리
	scwin.COR_TYPE_onchange = function(info) {
		var corType = COR_TYPE.getValue();
		
		if(corType == "CT001"){
			MOVE_TM.setDisabled(false);
		}else if (corType == "CT002" || corType == "CT003" || corType == "CT004" || corType == null || corType == ""){
			MOVE_TM.setDisabled(true);
		};
	};
	]]></script>
	</head>
	<body ev:onpageload="scwin.onpageload" ev:onpageunload="scwin.onpageunload">
		<xf:group id="pgm_container" class="pgm_container grdr_60_a" style="">
		
			<xf:group id="" class="grdc_2f_8f title-wrap" style="">
