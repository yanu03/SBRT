						<w2:column id="ST_STTN_NM" name="기점명" dataType="text"></w2:column>
						<w2:column id="ED_STTN_NM" name="종점명" dataType="text"></w2:column>
						<w2:column id="WAY_DIV" name="상하행" dataType="text"></w2:column>
