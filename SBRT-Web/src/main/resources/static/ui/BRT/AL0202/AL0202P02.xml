	//동적으로 datList 생성
	scwin.makeDataList = function(id, obj) {
		if(WebSquare.util.getComponentById(id)!=undefined){
			$w.data.remove(id);
		}
		$w.data.create({
			"id": id
			, "type": "dataList"
			, "option": {
				"useData": false
			  }
			, "columnInfo":obj
		});
	}
	
	//dataList 에 넣을 json
	scwin.setColumnID = function() {
		
		var column = {};
		var columnNmArr = ["REP_ROUT_ID","REP_ROUT_NM","DAY_DIV","ALLOC_NO","WAY_DIV"];
		var columnStrArr = ["대표노선ID","대표노선명","요일구분","코스","구분"];
		var columnArr = [];		
		var gridHeader ="", gridBody="";
		var rowData = dlt_OPER_ALLOC_PL_ROUT_INFO.getAllJSON();
		var rowCntArr = dlt_OPER_ALLOC_PL_ROUT_CNT.getAllJSON();
		
		var maxCnt = 0;
		
		for(var i = 0; i < rowCntArr.length; i++) {
			if(maxCnt < rowCntArr[i].CNT)
				maxCnt = rowCntArr[i].CNT;
		}
		
		if(maxCnt==0)maxCnt=30;
		
		
		
		for(var i = 0; i < columnNmArr.length; i++) {
			//gridView의 컬럼 정보 정의
			if(i>2){
				gridHeader +='<w2:column blockSelect="false" id="'+'h'+columnNmArr[i]+'" style="height:20px" inputType="text" width="70" value="'+columnStrArr[i]+'"></w2:column>';
				gridBody += '<w2:column blockSelect="false" id="'+columnNmArr[i]+'" style="height:20px" inputType="text" width="70"></w2:column>'
			}
		}
		
		if(rowData.length == 0 ) {
			gridHeader +='<w2:column blockSelect="false" id="h_OPER_INFO" style="height:25px; padding-left:350px" textAlign="left" inputType="text" width="100" colSpan="'+1+'" value="운행경로 및 운행시간"></w2:column>';
			var gridStr = '<w2:gridView xmlns:w2="http://www.inswave.com/websquare" xmlns:ev="http://www.w3.org/2001/xml-events" dataList="dlt_VIEW_OPER_PL_ROUT_INFO" '
							+ 'scrollByColumn="true" id="AL0202G1" class ="cm_grid right" fixedColumnWithHidden="true" useShiftKey="false" mergeCells="byrestriccol" mergeCol="ALLOC_NO,WAY_DIV">'
			+ '<w2:header id="header1" style="">'
			+ '<w2:row id="row1" style="">'
			+ gridHeader
			+ '</w2:row>'
			+'</w2:header>'
			+'<w2:gBody id="gBody1" style="">'
			+'<w2:row id="row2" style="">'
			+ gridBody
			+'</w2:row>'
			+'</w2:gBody>'
			+'</w2:gridView>' ;
			
			AL0202G1.setGridStyle(WebSquare.xml.parse( gridStr , true ));
			dlt_VIEW_OPER_PL_ROUT_INFO.removeAll();
			return;
		}
		
		dlt_VIEW_OPER_PL_ROUT_INFO.removeAll();
		gridHeader +='<w2:column blockSelect="false" id="" style="height:25px; padding-left:350px" textAlign="left" inputType="text" width="100" colSpan="'+maxCnt+'" value="운행경로 및 운행시간"></w2:column>';

		for(var i = 0; i < maxCnt; i++) {
			
			gridBody += '<w2:column blockSelect="false" id="'+i+'" style="height:20px" inputType="text" width="100"></w2:column>'
		}

		var rowIndex = 0;
		var rowCnt = 0;
		for(var i = 0; i < rowCntArr.length; i++) {
			rowIndex += rowCnt;
			rowCnt =  rowCntArr[i].CNT;
			
			var idx = dlt_VIEW_OPER_PL_ROUT_INFO.insertRow();
			for(var j = 0; j < columnNmArr.length; j++) {
				var columnVal ="";
				if(columnNmArr[j]=="WAY_DIV"){
					if(dlt_OPER_ALLOC_PL_ROUT_INFO.getCellData(rowIndex+j, columnNmArr[j]) == "WD001"){
						columnVal = dlt_OPER_ALLOC_PL_ROUT_INFO.getCellData(rowIndex+j, "WAY_ASC_NM");
					}
					else if(dlt_OPER_ALLOC_PL_ROUT_INFO.getCellData(rowIndex+j, columnNmArr[j]) == "WD002"){
						columnVal = dlt_OPER_ALLOC_PL_ROUT_INFO.getCellData(rowIndex+j, "WAY_DESC_NM");
					}
				}
				else if(columnNmArr[j]=="ALLOC_NO"){
					columnVal = "B"+dlt_OPER_ALLOC_PL_ROUT_INFO.getCellData(rowIndex+j, columnNmArr[j]);
				}
				else{
					columnVal = dlt_OPER_ALLOC_PL_ROUT_INFO.getCellData(rowIndex+j, columnNmArr[j]);
				}
				dlt_VIEW_OPER_PL_ROUT_INFO.setCellData(idx, columnNmArr[j], columnVal);
			}
			
			for(var j = 0; j < rowCnt; j++) {
				var routNm = dlt_OPER_ALLOC_PL_ROUT_INFO.getCellData(rowIndex+j, "ROUT_NM");
				var operSn = dlt_OPER_ALLOC_PL_ROUT_INFO.getCellData(rowIndex+j, "OPER_SN");
				dlt_VIEW_OPER_PL_ROUT_INFO.setCellData(idx, ""+j, routNm +"("+operSn+")");
			}
			
			var idx = dlt_VIEW_OPER_PL_ROUT_INFO.insertRow();
			for(var j = 0; j < columnNmArr.length; j++) {
				var columnVal ="";
				if(columnNmArr[j]=="WAY_DIV"){
					if(dlt_OPER_ALLOC_PL_ROUT_INFO.getCellData(rowIndex+j, columnNmArr[j]) == "WD001"){
						columnVal = dlt_OPER_ALLOC_PL_ROUT_INFO.getCellData(rowIndex+j, "WAY_ASC_NM");
					}
					else if(dlt_OPER_ALLOC_PL_ROUT_INFO.getCellData(rowIndex+j, columnNmArr[j]) == "WD002"){
						columnVal = dlt_OPER_ALLOC_PL_ROUT_INFO.getCellData(rowIndex+j, "WAY_DESC_NM");
					}
				}
				else if(columnNmArr[j]=="ALLOC_NO"){
					columnVal = "B"+dlt_OPER_ALLOC_PL_ROUT_INFO.getCellData(rowIndex+j, columnNmArr[j]);
				}
				else{
					columnVal = dlt_OPER_ALLOC_PL_ROUT_INFO.getCellData(rowIndex+j, columnNmArr[j]);
				}
				dlt_VIEW_OPER_PL_ROUT_INFO.setCellData(idx, columnNmArr[j], columnVal);
			}
			for(var j = 0; j < rowCnt; j++) {
				var routStTm = dlt_OPER_ALLOC_PL_ROUT_INFO.getCellData(rowIndex+j, "ROUT_ST_TM");
				var routEdTm = dlt_OPER_ALLOC_PL_ROUT_INFO.getCellData(rowIndex+j, "ROUT_ED_TM");
				dlt_VIEW_OPER_PL_ROUT_INFO.setCellData(idx, ""+j, routStTm+"~"+routEdTm);
			}
		}
		
		//gridView 정보 정의 
		//xml 형식을 정의 하면 <w2:gridView 에 xmlns:w2="http://www.inswave.com/websquare" xmlns:ev="http://www.w3.org/2001/xml-events" 포함 합니다.
		var gridStr = '<w2:gridView xmlns:w2="http://www.inswave.com/websquare" xmlns:ev="http://www.w3.org/2001/xml-events" dataList="dlt_VIEW_OPER_PL_ROUT_INFO" '
						+ 'scrollByColumn="true" id="AL0202G1" class ="cm_grid right" fixedColumnWithHidden="true" useShiftKey="false" mergeCells="byrestriccol" '
						+ 'mergeCol="ALLOC_NO,WAY_DIV" fixedColumn="2"  mergeCellsFixedCols="bycol" mergeCellsFixedRows="never">'
		+ '<w2:header id="header1" style="text-align:left">'
		+ '<w2:row id="row1" style="">'
		+ gridHeader
		+ '</w2:row>'
		+'</w2:header>'
		+'<w2:gBody id="gBody1" style="">'
		+'<w2:row id="row2" style="">'
		+ gridBody
		+'</w2:row>'
		+'</w2:gBody>'
		+'</w2:gridView>' ;
		
		AL0202G1.setGridStyle(WebSquare.xml.parse( gridStr , true ));
		
		autoOptions.Sub1.grid = AL0202G1;
		
		//AL0202G1.mergeCells("byrestriccol");
		//AL0202G1.setFixedColumn(2);
		//AL0202G1.mergeCellsFixedCols("bycol");
		
		//AL0202G1.mergeCells("byrestriccol");
		//AL0202G1.mergeCol("ALLOC_NO,WAY_DIV");
		//AL0202G1.setFixedColumn(2);

		dlt_VIEW_OPER_PL_ROUT_INFO.modifyAllStatus("R")
	}
		
