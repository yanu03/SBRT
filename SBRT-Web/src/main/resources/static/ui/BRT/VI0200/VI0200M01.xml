	scwin.VI0200G0_oncellclick = function(row, col, colId) {
		debugger;
		var gridColumn = VI0200G0.getColumnID(col);
		
		if (gridColumn == "LOCATION") {
				var param = VI0200G0.getDataRow(row).OCR_DTM;
				if (param != "") {
					dma_sub_search.set("OCR_DTM", param);
					dma_sub_search.set("VHC_NO",VI0200G0.getDataRow(row).VHC_NO);
					
					
					var data = { data : dma_sub_search.getJSON(), callbackFn : scwin.insertPopupData };
					
					var options = {
						id : "VI0200P01",
						popupName : "위치 지도",
						modal : true
					};
					com.openPopup(("/ui/BRT/VI0200/VI0200P01.xml"), options, data);
			}
		}
	};
	
	
