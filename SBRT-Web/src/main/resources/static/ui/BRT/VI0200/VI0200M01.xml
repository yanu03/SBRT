		};
		
		com.setMainBtn2(wfm_mainBtn, gcm.DISP_TYPE.SINGLE_GRID, autoOptions);
		
		var codeOptions = [ {code : "CPLNT_TYPE", compID : "VI0200G0:CPLNT_TYPE" } 
							,{code : "JUDG_TYPE", compID : "VI0200G0:JUDG_TYPE" }
					];
					
		com.setCommonDtl(codeOptions, function(){
			com.searchGrid(VI0200G0, sub_VI0200G0R0);
		});
		
		$p.executeSubmission(sub_VI0200G0R0);
		$p.executeSubmission(sub_VI0200SHI0);

	};

	scwin.onpageunload = function() {

	};
	scwin.sub_VI0200G0S0_submitdone = function(e) {
		com.saveDone(VI0200G0, sub_VI0200G0R0);	
	};
	
	scwin.sub_VI0200G0R0_submitdone = function(e) {
		var sCode = com.getResultCode(e);
		if (sCode == gcm.MESSAGE_CODE.STATUS_SUCCESS) {
			com.setFocusedCell(VI0200G0, "OCR_DTM");
		}
	};
	
	scwin.sbx_ChoiceBox_onviewchange = function(info) {		
		if (info.newValue == "ALL") {
			acb_autoChoiceBox.hide();
			search_input.setValue("");
			search_input.show("");
		} else if (info.newValue == "COMP_NM") {
			search_input.hide();
			acb_autoChoiceBox.setNodeSet("data:dlt_grgSearchItem", "COMP_NM", "COMP_NM");
			acb_autoChoiceBox.show("");
		} else if (info.newValue == "ROUT_NM") {
			search_input.hide();
			acb_autoChoiceBox.setNodeSet("data:dlt_grgSearchItem", "ROUT_NM", "ROUT_NM");
			acb_autoChoiceBox.show("");
		} else if (info.newValue == "CPLNT_TYPE") {
			search_input.hide();
			acb_autoChoiceBox.setNodeSet("data:dlt_grgSearchItem", "CPLNT_TYPE", "CPLNT_TYPE");
			acb_autoChoiceBox.show("");
		}
	};
	
	/*scwin.MO0302G0_onrowindexchange = function(row, oldRow) {
		com.changeSingleGrid(MO0302G0, row, oldRow);
	};*/

