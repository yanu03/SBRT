	};
	
	scwin.VI0100G1_onrowindexchange = function(row,oldRow) {
		//var curIndex = VI0100G1.getFocusedRowIndex();
		//var curVhcId = dlt_BRT_OPER_VIOLT_HIS.getCellData(curIndex, "VHC_ID");
		var jsonObj = dlt_BRT_OPER_VIOLT_HIS.getRowJSON(row);
		
		//차량 단일 마커
		routMap.showVehicle3("map_VI0100", jsonObj, VI0100G1);
	};
	
	//맵 클릭 이벤트
	scwin.onClickMap = function(e) {
		routMap.changeLocVehicleByClick("map_VI0100", VI0100G1, VI0100G1.getFocusedRowIndex(),e);
	}
	
	scwin.VI0100G0_onrowindexchange = function(row,oldRow) {
		dma_param_VI0100G1.set("OPER_DT", VI0100G0.getCellData(row, "OPER_DT"));
		dma_param_VI0100G1.set("REP_ROUT_ID", VI0100G0.getCellData(row, "REP_ROUT_ID"));
		dma_param_VI0100G1.set("ROUT_ID", VI0100G0.getCellData(row, "ROUT_ID"));
		dma_param_VI0100G1.set("VHC_ID", VI0100G0.getCellData(row, "VHC_ID"));
		dma_param_VI0100G1.set("VHC_NO", VI0100G0.getCellData(row, "VHC_NO"));
		
		var focusOption = {keyMap : dma_param_VI0100G1};
		com.changeDualGrid(VI0100G0, VI0100G1, sub_VI0100G1S0, sub_VI0100G1R0, focusOption, row, oldRow, "위반 이력 목록");
	};
	
	scwin.sub_VI0100G1S0_submitdone = function(e) {
		com.saveDone(VI0100G1, sub_VI0100G1R0);		
	};
	
	scwin.sbx_ChoiceBox_onviewchange = function(info) {		
		if (info.newValue == "ALL") {
			acb_autoChoiceBox.hide();
			search_input.setValue("");
			search_input.show("");
		} else if (info.newValue == "REP_ROUT_ID") {
			search_input.hide();
			acb_autoChoiceBox.setNodeSet("data:dlt_searchitem", "REP_ROUT_ID", "REP_ROUT_ID");
			acb_autoChoiceBox.show("");
		} else if (info.newValue == "REP_ROUT_NM") {
			search_input.hide();
			acb_autoChoiceBox.setNodeSet("data:dlt_searchitem", "REP_ROUT_NM", "REP_ROUT_NM");
			acb_autoChoiceBox.show("");
		}
		
	};
	
	scwin.btn_searchNode_onclick = function(e) {
		var data = { NODE_ID : dlt_BRT_OPER_VIOLT_HIS.getCellData(VI0100G1.getFocusedRowIndex(), "NODE_ID"),
				   	 callbackFn : "scwin.searchNodeCallback" };
				   	 
		var opt = { id : "VI0100P01", 
					popupName : "인근장소 검색",
					width : 600,
					height : 500,
					modal : true};
		com.openPopup("/ui/BRT/VI0100/VI0100P01.xml", opt, data);		
	};
	
	//찾기 버튼 콜백함수
