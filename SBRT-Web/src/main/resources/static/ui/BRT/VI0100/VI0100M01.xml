		var codeOptions = [ {code : "AREA", compID : "VI0100G0:AREA" }
							,{code : "VIOLT_TYPE", compID : "VIOLT_TYPE, VI0100G1:VIOLT_TYPE" }
							,{code : "JUDG_TYPE", compID : "JUDG_TYPE, VI0100G1:JUDG_TYPE" }
						];		
	
		com.setCommonDtl(codeOptions, function() {
			com.searchGrid(VI0100G0, sub_VI0100G0R0);
		});	
		
		routMap.initMap("map_VI0100", {width:"100%"
			, height:"100%"
			, onClick: scwin.onClickMap
			, nodeChangeCb: null
			, draggable: false
		});
		
		$p.executeSubmission(sub_VI0100SHI0);
		
	};
	
	scwin.onpageunload = function() {
		
	};
	
