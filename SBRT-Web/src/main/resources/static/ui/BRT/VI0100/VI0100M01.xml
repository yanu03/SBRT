		NODE_NM.setValue(returnValue[0].NODE_NM);
		dlt_BRT_OPER_VIOLT_HIS.setCellData(VI0100G1.getFocusedRowIndex(), "NODE_ID", returnValue[0].NODE_ID);
	};
	
	scwin.NODE_NM_onfocus = function(e) {
		scwin.btn_searchNode_onclick();		
	};
	
	
