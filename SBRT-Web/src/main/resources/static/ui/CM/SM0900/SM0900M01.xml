		<script type="text/javascript" lazy="false"><![CDATA[
	/**
	 * 초기 스크립트 수행 함수 정의
	 * sbm_ProgramSearchItem은 검색박스에 검색어들을 나열한다.
	 */
	scwin.onpageload = function() {
	
		var autoOptions = {
			Main : { grid : grd_memberBasic, srchSbm : sbm_searchMemberBasic, srchGrp : pgm_header, 
					keyColumn : "USER_ID", focusColumn : "USER_ID", name :  "프로그램"},
			Sub1 : { grid : grd_program, srchSbm : sbm_searchUserAuthProgram, savSbm : sbm_saveUserAuthProgram , 
					keyColumn : "PROG_CD", focusColumn : "PROG_GRP_CD", name :  "프로그램별 접근권한"}
		 };
		
		//추가, 삭제, 취소 버튼 안보이게 설정
		var mainOptions ={
			ADD:{}, DEL:{},CNL:{}
		} 
		
		com.setMainBtn2(wfm_mainBtn, gcm.DISP_TYPE.DUAL_GRID, autoOptions,mainOptions);
		
		var subOptions = {
			ADD: scwin.btn_addProgramAuthority_onclick, DEL:{}, CNL:{}, EXL:{}, SAVE:{}
		};
		com.setSubBtn2(wfm_subBtn, autoOptions, subOptions);	
		
		com.executeSubmission(sbm_searchMemberBasicOrganization);
		
		var codeOptions = [{ code : "PROG_GRP_CD", compID : "grd_program:PROG_GRP_CD"}];
		
		com.setCommonDtl(codeOptions, function() {
				com.searchGrid(grd_memberBasic,sbm_searchMemberBasic);
		});
		
		com.executeSubmission(sbm_memberSearchItem);
		
	};

	scwin.onpageunload = function() {

	};
	
	/**
	 * 사용자정보 조회 콜백 메소드
	 */
	scwin.sbm_searchMemberBasic_submitdone = function(e) {
		var sCode = com.getResultCode(e);
		if (sCode == gcm.MESSAGE_CODE.STATUS_SUCCESS) {
			com.setFocusedCell(grd_memberBasic, "USER_ID");
		}
	};
	
	/**
	 * 사용자 정보 리스트의 row 인덱스 위치가 바뀔때 마다 실행되며 사용자별 권한을 가지고 있는 프로그램을 가져온다.
	 */
	scwin.grd_memberBasic_onrowindexchange = function(row,oldRow) {
		var focusOption = {keyMap : dma_program, keyMapColumn : "USER_ID"};
		
		com.changeDualGrid(grd_memberBasic, grd_program, sbm_saveUserAuthProgram,
					sbm_searchUserAuthProgram, focusOption , row, oldRow, "프로그램별 접근 권한");
	};
	
	/**
	 * 프로그램 조회 버튼 콜백 메소드
	 */
	scwin.sbm_searchProgram_submitdone = function(e) {
		var sCode = com.getResultCode(e);
		if (sCode == gcm.MESSAGE_CODE.STATUS_SUCCESS) {
			com.setFocusedCell(grd_program, "PROG_GRP_CD");
		}
	};

	/**
	 * 프로그램리스트 및 프로그램별 접근권한 리스트 저장 후 callback
	 */
	scwin.sbm_saveProgramAll_submitdone = function(e) {
		var rsCode = com.getResultCode(e)

		if (rsCode == gcm.MESSAGE_CODE.STATUS_SAVE) {
	
			com.saveDone(grd_program,sbm_searchProgram,grd_programAuthority);
		}
	};

	/**
	 * 프로그램관리 저장 버튼의 콜백메소드
	 */
	scwin.sbm_saveProgram_submitdone = function(e) {
		var sCode = com.getResultCode(e);
		if (sCode == gcm.MESSAGE_CODE.STATUS_SAVE) {
			com.saveDone(grd_program,sbm_searchUserAuthProgram);
		}
	};
	
	/**
	 * 프로그램별 접근 프로그램 리스트에서 등록 버튼클릭시 수행되며, 프로그램별 접근 프로그램 등록 페이지를 layer popup으로 표현해준다.
	 */
	scwin.btn_addProgramAuthority_onclick = function() {
		var focusRowIdx = grd_memberBasic.getFocusedRowIndex();
		var progCdIds = dlt_userAuthProgram.getColData("PROG_CD");
		dma_program.set("PROG_CD_IDS", progCdIds);
		
		if (focusRowIdx == -1) {
			com.alert("사용자 선택 후 프로그램별 접근 권한을 등록하실 수 있습니다.");
		} else {
			var rowStatus = grd_memberBasic.getRowStatus(focusRowIdx);
			if (rowStatus == "C") {
				com.alert("사용자 등록 후에 사용 가능합니다.");
			} else {
				var userId = dma_program.get("USER_ID");
				if (userId != "") {
					var data = {data : dma_program.getJSON(), callbackFn : "scwin.insertProgramAuthority" };
					var options = {
						id : "ProgramAuthorityPop",
						popupName : "프로그램별 접근 권한",
						modal : true,
						width : 1100, height: 550 };
					com.openPopup(com.getFullPath("/ui/CM/SM0900/SM0900P01.xml"), options, data);
				}
			}
		}
	};
	
	/**
	 * 팝업창에 선택된 프로그램 정보를 DataList( dlt_userAuthProgram )에 추가시킨다.
	 */
	scwin.insertProgramAuthority = function(obj) {
		debugger;
		var dLength = obj.length;
		for (i = 0; i < dLength; i++) {
			var data = dlt_userAuthProgram.getMatchedIndex("PROG_CD", obj[i].PROG_CD, true);
			if (data.length == 0) {
				//obj[i].PROG_CD = dma_program.get("PROG_CD");
				obj[i].chk = "0";
				obj[i].USER_ID = dlt_memberBasic.getCellData(grd_memberBasic.getFocusedRowIndex(), "USER_ID");
				var idx = dlt_userAuthProgram.insertRow();
				dlt_userAuthProgram.setRowJSON(idx, obj[i], true);
			}else{
				com.alert("이미 추가된 프로그램입니다.");
			}
		}
		
	};

	/**
	 * 프로그램별 접근 프로그램 콜백메소드
	 */
	scwin.sbm_saveProgramAuthority_submitdone = function(e) {
		var sCode = com.getResultCode(e);
		if (sCode == gcm.MESSAGE_CODE.STATUS_SAVE) {
			com.saveDone(grd_programAuthority,sbm_searchProgramAuthority);
		}			
	};

	/**
	 * 검색항목(selectBox)의 내용이 변경되었을 때 실행된다.
	 * 직위면 inputBox를 hidden, selectBox를 띄운다. 
	 */
	scwin.sbx_ChoiceBox_onviewchange = function(info) {		
		if (info.newValue == "ALL") {
			acb_autoChoiceBox.hide();
			search_input.setValue("");
			search_input.show("");
		} else if (info.newValue == "USER_ID") {
			search_input.hide();
			acb_autoChoiceBox.setNodeSet("data:dlt_memberSearchItem", "USER_ID", "USER_ID");
			acb_autoChoiceBox.show("");
		} else if (info.newValue == "USER_NM") {
			search_input.hide();
			acb_autoChoiceBox.setNodeSet("data:dlt_memberSearchItem", "USER_NM", "USER_NM");
			acb_autoChoiceBox.show("");
		}
	};
	
	]]></script>
	</head>
